    <rules>
        <fullName>OHRM - Date To Send First Email</fullName>
        <actions>
            <name>OHRM_Update_Start_Date_Email_intiation</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>RecordType.Name = &apos;GSA&apos; &amp;&amp;  NOT(ISCHANGED(Start_Date_Email_Initiation__c ))  &amp;&amp;  NOT(ISBLANK (Person_Start_Date__c )) &amp;&amp; ISCHANGED( Person_Start_Date__c )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>OHRM - Date To Send First Email %28creation%29</fullName>
        <actions>
            <name>OHRM_Update_Start_Date_Email_intiation</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>RecordType.Name = &apos;GSA&apos; &amp;&amp;  ISBLANK(Start_Date_Email_Initiation__c )  &amp;&amp;  NOT(ISBLANK (Person_Start_Date__c ))</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>OHRM-AffiliationCopy</fullName>
        <actions>
            <name>OHRM_AffiliationCopy</name>
            <type>FieldUpdate</type>
        </actions>
