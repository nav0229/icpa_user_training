    <rules>
        <fullName>VOE - Contact - Update New Hire Text Statuses</fullName>
        <actions>
            <name>VOE_Contact_NHTextStatusFromQualtrics</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>After the contact has responded to the survey via Qualtrics, append the New Hire Text Status field with Response Mapping from Qualtrics</description>
        <formula>ISCHANGED( NewHireTextStatusQualtrics__c )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>VOE - New Hire Workflow 180</fullName>
        <active>true</active>
        <booleanFilter>1 AND 2 AND 3 AND 4 AND 5 AND 6 AND 7 AND 8 AND 9</booleanFilter>
        <criteriaItems>
            <field>Contact.Email</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Contact.isActive__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>Contact.NewHireEmailTextStatuses__c</field>
            <operation>notContain</operation>
            <value>NH_180 Day Sent</value>
        </criteriaItems>
        <criteriaItems>
            <field>Contact.RecordTypeId</field>
            <operation>equals</operation>
            <value>GSA</value>
        </criteriaItems>
        <criteriaItems>
            <field>Contact.MAJOR_ORG__c</field>
            <operation>notEqual</operation>
            <value>G</value>
        </criteriaItems>
        <criteriaItems>
            <field>Contact.MAJOR_ORG__c</field>
            <operation>notEqual</operation>
            <value>J</value>
        </criteriaItems>
        <criteriaItems>
            <field>Contact.Affiliation__c</field>
            <operation>notEqual</operation>
            <value>Contractor</value>
        </criteriaItems>
        <criteriaItems>
            <field>Contact.Days_from_Hire_Date__c</field>
            <operation>lessOrEqual</operation>
            <value>180</value>
        </criteriaItems>
        <criteriaItems>
            <field>Contact.Person_Start_Date__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <description>Queue Time-Based Action for 180 day New Hire Survey.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>VOE_New_Hire_Email_Status_Update_180_Day</name>
                <type>FieldUpdate</type>
            </actions>
            <actions>
                <name>VOE_New_Hire_Workflow_180</name>
                <type>OutboundMessage</type>
            </actions>
            <actions>
                <name>VOE_New_Hire_180_Day_Sent_Task</name>
                <type>Task</type>
            </actions>
            <offsetFromField>Contact.Person_Start_Date__c</offsetFromField>
            <timeLength>180</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
