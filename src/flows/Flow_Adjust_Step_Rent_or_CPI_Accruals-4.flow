<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <actionCalls>
        <name>Flow_Fault_Email_Message</name>
        <label>Flow Fault Email Message</label>
        <locationX>513</locationX>
        <locationY>755</locationY>
        <actionName>emailSimple</actionName>
        <actionType>emailSimple</actionType>
        <inputParameters>
            <name>emailBody</name>
            <value>
                <elementReference>ErrorFlowFaultMessage</elementReference>
            </value>
        </inputParameters>
        <inputParameters>
            <name>emailSubject</name>
            <value>
                <stringValue>Flow Error: G2 - Adjust Step Rent or CPI Accruals</stringValue>
            </value>
        </inputParameters>
        <inputParameters>
            <name>emailAddresses</name>
            <value>
                <stringValue>Galaxy2Help@unisys.com</stringValue>
            </value>
        </inputParameters>
    </actionCalls>
    <assignments>
        <description>1) Missing Funding Payment - This is a invalid business case scenario ... 

2) Accrual Amount &gt; Prior Year Accrual amount - no adjustments needed ...

we want to exit the flow with no operations ...</description>
        <name>Exit_Flow_With_No_Adjustments</name>
        <label>Exit Flow With No Adjustments</label>
        <locationX>915</locationX>
        <locationY>460</locationY>
        <assignmentItems>
            <assignToReference>dblAdjustedAmount</assignToReference>
            <operator>Assign</operator>
            <value>
                <numberValue>0.0</numberValue>
            </value>
        </assignmentItems>
    </assignments>
    <assignments>
        <name>Lump_Sum_Amount_Prior_Year_Accrual_Amount</name>
        <label>Lump Sum Amount = Prior Year Accrual Amount</label>
        <locationX>175</locationX>
        <locationY>522</locationY>
        <assignmentItems>
            <assignToReference>dblAdjustedAmount</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>dblPYAccrualAmount</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Is_the_Adjustment_Amount_0</targetReference>
        </connector>
    </assignments>
    <assignments>
        <name>Lump_Sum_Amount_Prior_Year_Accrual_Amount_Digest_Amount</name>
        <label>Lump Sum Amount = (Prior Year Accrual Amount - Digest Amount)</label>
        <locationX>459</locationX>
        <locationY>522</locationY>
        <assignmentItems>
            <assignToReference>dblAdjustedAmount</assignToReference>
            <operator>Assign</operator>
            <value>
                <elementReference>dblDiffAmount</elementReference>
            </value>
        </assignmentItems>
        <connector>
            <targetReference>Is_the_Adjustment_Amount_0</targetReference>
        </connector>
    </assignments>
    <decisions>
        <name>Adjust_the_Accruals</name>
        <label>Adjust the Accruals</label>
        <locationX>313</locationX>
        <locationY>405</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>No_Accruals_Specified</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>dblAccrualAmountonDigest</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Lump_Sum_Amount_Prior_Year_Accrual_Amount</targetReference>
            </connector>
            <label>No Accruals Specified</label>
        </rules>
        <rules>
            <name>Accruals_Amount_Greater_than_equal_to_Prior_Year_Accrual</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>dblAccrualAmountonDigest</leftValueReference>
                <operator>GreaterThanOrEqualTo</operator>
                <rightValue>
                    <elementReference>dblPYAccrualAmount</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Exit_Flow_With_No_Adjustments</targetReference>
            </connector>
            <label>Accruals Amount Greater than equal to Prior Year Accrual</label>
        </rules>
        <rules>
            <name>Accruals_Amount_less_than_Prior_Year_Accrual</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>dblAccrualAmountonDigest</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <elementReference>dblPYAccrualAmount</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Lump_Sum_Amount_Prior_Year_Accrual_Amount_Digest_Amount</targetReference>
            </connector>
            <label>Accruals Amount less than Prior Year Accrual</label>
        </rules>
    </decisions>
    <decisions>
        <name>Funding_Payment_Found</name>
        <label>Funding Payment Found?</label>
        <locationX>314</locationX>
        <locationY>145</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>objFunding</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Look_Up_Lump_Sum_Record_Type_ID</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
        <rules>
            <name>No</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>objFunding</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Exit_Flow_With_No_Adjustments</targetReference>
            </connector>
            <label>No</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_the_Adjustment_Amount_0</name>
        <label>Is the Adjustment Amount != 0?</label>
        <locationX>320</locationX>
        <locationY>614</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Adj_Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>dblAdjustedAmount</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Create_Lump_Sum_to_adjust_the_Accruals</targetReference>
            </connector>
            <label>Yes</label>
        </rules>
        <rules>
            <name>Adj_No</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>dblAdjustedAmount</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <numberValue>0.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Exit_Flow_With_No_Adjustments</targetReference>
            </connector>
            <label>No</label>
        </rules>
    </decisions>
    <formulas>
        <name>dblAccrualAmountonDigest</name>
        <dataType>Currency</dataType>
        <expression>{!objFunding.LeaseDigest__r.AccrualAmountForDigest__c}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>dblDiffAmount</name>
        <dataType>Currency</dataType>
        <expression>{!(dblPYAccrualAmount - objFunding.LeaseDigest__r.AccrualAmountForDigest__c)}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>strComments</name>
        <dataType>String</dataType>
        <expression>&apos;  [** &apos; &amp;
&apos;Record created by the Accrual Process on &apos; &amp; Text(Today()) &amp; &apos;. &apos; &amp;
&apos;Transaction ID = &apos; &amp; 
{!objFunding.LeaseDigest__r.Transaction__r.Name} &amp; &apos;; &apos; &amp;
&apos;PDN = &apos; &amp; 
{!objFunding.LeaseDigest__r.PDNsForDigest__c} &amp; &apos;; &apos; &amp;
 &apos;**]&apos;</expression>
    </formulas>
    <interviewLabel>Flow - Adjust Step Rent or CPI Accruals {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Flow - Adjust Step Rent or CPI Accruals</label>
    <processType>AutoLaunchedFlow</processType>
    <recordCreates>
        <name>Create_Lump_Sum_to_adjust_the_Accruals</name>
        <label>Create Lump Sum to adjust the Accruals</label>
        <locationX>320</locationX>
        <locationY>751</locationY>
        <faultConnector>
