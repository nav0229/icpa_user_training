                <elementReference>strLeaseID</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>LumSumType__c</field>
            <value>
                <stringValue>Catch Up Payment</stringValue>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>ProcessedDate__c</field>
