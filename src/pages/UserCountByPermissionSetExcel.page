<!-- OCIO CASE # 00002867                                                                                          -->
<!-- Custom Visualforce Page to display the users count by permission set in EXCEL sheet                           -->
<!---------------------------------------------------------------------------------------------------------------- -->  
<!-- Author             |Date       |Version   | Description                                                       -->
<!---------------------------------------------------------------------------------------------------------------- -->
<!-- Rajakumar P         09/11/2012  1.0         Extract the user count report to excel sheet                      -->
<!---------------------------------------------------------------------------------------------------------------- --> 

<apex:page controller="UserCountController" contenttype="application/vnd.ms-excel#UserCountByPermissionSet.xls" cache="true" readonly="true">
   <apex:pageBlock title="OCIO Users Count by PermissionSet" >
      <apex:pageBlockTable value="{!Results}" var="r">
          <apex:column headerValue="Permission Set" value="{!r.PermSetName}"/>
          <apex:column dir="RTL" headerValue="Number of Users" value="{!r.TotalUsers}"/> 
      </apex:pageBlockTable>
   </apex:pageBlock>
   <!-- <apex:include pageName="UserCountByPermissionSet"/> -->
</apex:page>