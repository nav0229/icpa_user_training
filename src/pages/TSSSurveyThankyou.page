<apex:page sidebar="false" showheader="false" title="GSA" standardstylesheets="false" doctype="html-5.0" id="TSSthanks">
<apex:stylesheet value="{!URLFOR($Resource.wds, '/uswds-0.9.0/css/uswds.min.css')}"/>

    <style type="text/css">
        body {
          margin: 10px;

        }
        #TSSthanks:j_id3{
            background-color:#fff;!important
        }
        div.bpageHeader {
            display: none;
        }
        body.bpageFooter {
            display: none;
        }
        body #AppBodyHeader { display: none; }


    </style>

  <apex:image url="{!URLFOR($Resource.GSATSSLogo)}" alt="GSA seal" width="286" height="50" />
  <!-- THANK YOU MESSAGE BETWEEN 7/29/2015 and 9/11/2015 COB -->
  <!-- THANK YOU MESSAGE BETWEEN <06/28/2016> and <07/18/2016> COB edited by KP -->
  <!-- O&M Case# 26516 - Raja Kumar - criteria effective FROM 06/28/2016 to 08/02/2016 COB -->
  <!-- O&M Case# 26957 - Raja Kumar - criteria effective FROM 06/28/2016 to 08/09/2016 COB -->
  <div id="msg1">
  <apex:outputPanel id="msg" layout="block"  rendered="{!IF(OR(AND( DAY(TODAY()) > 9, MONTH(TODAY()) == 8, YEAR(TODAY()) == 2016 ),
                                                              AND( MONTH(TODAY()) > 8, YEAR(TODAY()) == 2016),
                                                              AND( YEAR(TODAY()) > 2016)),
                                                              false, true)}">

      <apex:pageBlock >
          <p style="font-style:Arial;font-size:1.7rem;text-align:left;margin-left:25px;margin-top:25px;margin-bottom:25px;">
              On behalf of GSA, thank you for your feedback. This survey provides us with actionable information that we will utilize to 
              improve your experience in Federal buildings.
              <br/><br/>
              Thank you,
              <br/><br/>
              <apex:image url="{!URLFOR($Resource.TSSThankyouSignature)}" alt="TSS Signature" width="163" height="94" />
              <br/><br/>
              Norman Dong<br/>
              GSA Public Buildings Service Commissioner
          </p>
      </apex:pageBlock>
  </apex:outputPanel>
  </div>

  <!-- END OF SURVEY MESSAGE FROM 9/12/2015 to 9/18/2015 COB -->
  <!-- END OF SURVEY MESSAGE FROM <7/19/2016> to <TBD> COB edited by KP -->
