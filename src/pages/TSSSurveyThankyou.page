  <!-- O&M Case# 26957 - Raja Kumar - criteria effective FROM 08/10/2016 TO <TBD> -->
  <apex:outputPanel id="msg2" layout="block" rendered="{!IF(OR(AND( DAY(TODAY()) > 9, MONTH(TODAY()) == 8, YEAR(TODAY()) == 2016 ),
                                                              AND( MONTH(TODAY()) > 8, YEAR(TODAY()) == 2016),
                                                              AND( YEAR(TODAY()) > 2016)),
                                                              true, false)}">
      <apex:pageBlock >
          <p style="font-style:Arial;font-size:1.7rem;text-align:left;margin-left:25px;margin-top:25px;margin-bottom:25px;">
              Thank you for your interest in the FY16 Federal Tenant Satisfaction Survey. This survey has closed and no additional feedback is being accepted at this time. <br/><br/>
              Thank you again for your interest and we look forward to hearing from you next year.  <br/><br/>
          </p>
      </apex:pageBlock>
  </apex:outputPanel>
