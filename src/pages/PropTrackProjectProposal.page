<script >
    function validateForm() {
        setErrorMessages();
        if (!checkEmail()) {
            document.getElementById('email').focus();
            return false;
        } else if (checkAnswer() == 0) {
            document.getElementById('captchamsg').style.display = "block";
            document.getElementById('inptHuman').focus();
            return false;
        } else if (isMissingCIOCTOContact()){
            document.getElementById('CTO_CIO_Contact__c').focus();
            return false;
        } else if (isMissingPPOther()) {
            document.getElementById('PPOtherInput').focus();
            return false;
        } else if (isMissingCIOCTOOther__c()) {
            document.getElementById('CIOCTOOtherInput').focus();
            return false;
        } else if (isMissingETOther()) {
            document.getElementById('ETOtherInput').focus();
            return false;
        }
    }
    function setErrorMessages() {
        hideOrUnhideElement('emailmsg', checkEmail());
        hideOrUnhideElement('captchamsg', !(checkAnswer()==0));
        hideOrUnhideElement('cioctomsg', !isMissingCIOCTOContact());
        redOrUnredElement('PPOOtherHT', isMissingPPOther());
        redOrUnredElement('CIOCTOtherHT', isMissingCIOCTOOther__c());
        redOrUnredElement('ETOtherHT', isMissingETOther());
    }

    function hideOrUnhideElement(element, hide) {
        document.getElementById(element).style.display = hide ? "none" : "block";

    }
    function redOrUnredElement(element, turnRed) {
        document.getElementById(element).style.color = turnRed ? "red" : "black";
    }
    function checkEmail() {
        var Email = document.getElementById('email').value;
        var filter = /^([a-zA-Z0-9_\.\-])+\@(([a-zA-Z0-9\-])+\.)+(gov|us|mil|GOV|US|MIL)+$/;
        return (filter.test(Email));
    }

    function isMissingCIOCTOContact() {
        return ((document.getElementById('YesCIOCTO').checked)&&((document.getElementById('CTO_CIO_Contact__c').value=="")||(document.getElementById('CTO_CIO_Contact__c').value==null)));
    }
    function isMissingCIOCTOOther__c() {
        return ((document.getElementById('OtherCIOCTO').checked)&&((document.getElementById('CIOCTOOtherInput').value=="")||(document.getElementById('CIOCTOOtherInput').value==null)));
    }
    function isMissingPPOther() {
       return ((document.getElementById('PPOtherRadio').checked)&&((document.getElementById('PPOtherInput').value=="")||(document.getElementById('PPOtherInput').value==null)));
    }
    function isMissingETOther() {
        return ((document.getElementById('Engagement_Timeframe__c').value=="Other")&&((document.getElementById('ETOtherInput').value=="")||(document.getElementById('ETOtherInput').value==null)));
    }

    function enableETOther() {
        if(document.getElementById('Engagement_Timeframe__c').value=="Other") {
            document.getElementById('ETOtherInput').style.display = "block";
        } else {
            document.getElementById('ETOtherInput').style.display = "none";
        }
    }
    function enableOther(button, input) {
        if(document.getElementById(button).checked) {
            document.getElementById(input).style.display = "block";
        } else {
            document.getElementById(input).style.display = "none";
        }
    }

        </script>
