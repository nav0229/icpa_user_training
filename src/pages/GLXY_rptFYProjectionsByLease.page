			document.getElementById('{!$Component.frmLease.Total.GLXY_TOTSEP}').innerHTML = TotalAmountSEP;
			document.getElementById('{!$Component.frmLease.Total.GLXY_TOTCYT}').innerHTML = TotalAmountCYT;
		}
				
		function numberWithCommas(x) {
    		return x.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
		}
