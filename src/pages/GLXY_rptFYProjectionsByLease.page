 	<script language = "Javascript">
	
 		var blnValidLease = 'false'; //Assume no lease found ...
		var Amount = '0';
		var TotalAmountSFT = '0';
		var TotalAmountOCT = '0';
		var TotalAmountNOV = '0';
		var TotalAmountDEC = '0';
		var TotalAmountJAN = '0';
		var TotalAmountFEB = '0';
		var TotalAmountMAR = '0';
		var TotalAmountAPR = '0';
		var TotalAmountMAY = '0';
		var TotalAmountJUN = '0';
		var TotalAmountJUL = '0';
		var TotalAmountAUG = '0';
		var TotalAmountSEP = '0';
		var TotalAmountCYT = '0';
		
 		var elems = document.getElementsByTagName("*");
		
 		for (var i=0, m=elems.length; i<m; i++) {
			if (elems[i].id && elems[i].id.indexOf(":GLXY_") != -1) {
				blnValidLease = 'true';
	 			Amount = document.getElementById(elems[i].id).innerHTML;
			    Amount = Amount.replace('$','');
			    Amount = Amount.replace('$','').replace(',','').replace(',','').replace(',','').replace(' ',''); // Easy way to make sure that we handle big numbers, or more commas ...
		    }
		    if (elems[i].id && elems[i].id.indexOf(":GLXY_SFT") != -1) {
		    	TotalAmountSFT = parseInt(TotalAmountSFT, 10) + parseInt(Amount, 10);
		 	}
		    if (elems[i].id && elems[i].id.indexOf(":GLXY_OCT") != -1) {
		    	TotalAmountOCT = parseInt(TotalAmountOCT) + parseInt(Amount);
		 	}
		 	if (elems[i].id && elems[i].id.indexOf(":GLXY_NOV") != -1) {
		    	TotalAmountNOV = parseInt(TotalAmountNOV, 10) + parseInt(Amount, 10);
		 	}
		 	if (elems[i].id && elems[i].id.indexOf(":GLXY_DEC") != -1) {
		    	TotalAmountDEC = parseInt(TotalAmountDEC, 10) + parseInt(Amount, 10);
		 	}
		 	if (elems[i].id && elems[i].id.indexOf(":GLXY_JAN") != -1) {
		    	TotalAmountJAN = parseInt(TotalAmountJAN, 10) + parseInt(Amount, 10);
		 	}
		 	if (elems[i].id && elems[i].id.indexOf(":GLXY_FEB") != -1) {
		    	TotalAmountFEB = parseInt(TotalAmountFEB, 10) + parseInt(Amount, 10);
		 	}
		 	if (elems[i].id && elems[i].id.indexOf(":GLXY_MAR") != -1) {
		    	TotalAmountMAR = parseInt(TotalAmountMAR, 10) + parseInt(Amount, 10);
		 	}
		 	if (elems[i].id && elems[i].id.indexOf(":GLXY_APR") != -1) {
		    	TotalAmountAPR = parseInt(TotalAmountAPR, 10) + parseInt(Amount, 10);
		 	}
		 	if (elems[i].id && elems[i].id.indexOf(":GLXY_MAY") != -1) {
		    	TotalAmountMAY = parseInt(TotalAmountMAY, 10) + parseInt(Amount, 10);
		 	}
		 	if (elems[i].id && elems[i].id.indexOf(":GLXY_JUN") != -1) {
		    	TotalAmountJUN = parseInt(TotalAmountJUN, 10) + parseInt(Amount, 10);
		 	}
		 	if (elems[i].id && elems[i].id.indexOf(":GLXY_JUL") != -1) {
		    	TotalAmountJUL = parseInt(TotalAmountJUL, 10) + parseInt(Amount, 10);
		 	}
		 	if (elems[i].id && elems[i].id.indexOf(":GLXY_AUG") != -1) {
		    	TotalAmountAUG = parseInt(TotalAmountAUG, 10) + parseInt(Amount, 10);
		 	}
		 	if (elems[i].id && elems[i].id.indexOf(":GLXY_SEP") != -1) {
		    	TotalAmountSEP = parseInt(TotalAmountSEP, 10) + parseInt(Amount, 10);
		 	}
		 	if (elems[i].id && elems[i].id.indexOf(":GLXY_CYT") != -1) {
		 		TotalAmountCYT = parseInt(TotalAmountCYT, 10) + parseInt(Amount, 10);
		 	}
		}
		
		if(blnValidLease=='true') {
			TotalAmountSFT = numberWithCommas(TotalAmountSFT);
			TotalAmountOCT = '$' + numberWithCommas(TotalAmountOCT);
			TotalAmountNOV = '$' + numberWithCommas(TotalAmountNOV);
			TotalAmountDEC = '$' + numberWithCommas(TotalAmountDEC);
			TotalAmountJAN = '$' + numberWithCommas(TotalAmountJAN);
			TotalAmountFEB = '$' + numberWithCommas(TotalAmountFEB);
			TotalAmountMAR = '$' + numberWithCommas(TotalAmountMAR);
			TotalAmountAPR = '$' + numberWithCommas(TotalAmountAPR);
			TotalAmountMAY = '$' + numberWithCommas(TotalAmountMAY);
			TotalAmountJUN = '$' + numberWithCommas(TotalAmountJUN);
			TotalAmountJUL = '$' + numberWithCommas(TotalAmountJUL);
			TotalAmountAUG = '$' + numberWithCommas(TotalAmountAUG);
			TotalAmountSEP = '$' + numberWithCommas(TotalAmountSEP);
			TotalAmountCYT = '$' + numberWithCommas(TotalAmountCYT);
			
			document.getElementById('{!$Component.frmLease.Total.GLXY_TOTSFT}').innerHTML = TotalAmountSFT;
			document.getElementById('{!$Component.frmLease.Total.GLXY_TOTOCT}').innerHTML = TotalAmountOCT;
			document.getElementById('{!$Component.frmLease.Total.GLXY_TOTNOV}').innerHTML = TotalAmountNOV;
			document.getElementById('{!$Component.frmLease.Total.GLXY_TOTDEC}').innerHTML = TotalAmountDEC;
			document.getElementById('{!$Component.frmLease.Total.GLXY_TOTJAN}').innerHTML = TotalAmountJAN;
			document.getElementById('{!$Component.frmLease.Total.GLXY_TOTFEB}').innerHTML = TotalAmountFEB;
