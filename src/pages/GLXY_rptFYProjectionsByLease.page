    				<apex:outputLabel value="{!$ObjectType.Glxy_Lease__c.fields.BaseAnnualRent__c.label}"></apex:outputLabel>
    				<apex:outputText value="{0, number, $###,###,##0}"><apex:param value="{!LeaseInfo.BaseAnnualRent__c}" /></apex:outputText>
    			</apex:pageBlockSectionItem>
    			<apex:pageBlockSectionItem >
    				<apex:outputLabel value="{!$ObjectType.Glxy_Lease__c.fields.BaseSquareFeet__c.label}"></apex:outputLabel>
