    			</apex:pageBlockSectionItem>
    			<apex:pageBlockSectionItem >
    				<apex:outputLabel value="{!$ObjectType.Glxy_Lease__c.fields.BaseLeaseEffectiveDate__c.label}"></apex:outputLabel>
    				<apex:outputText value="{0,date,MM/dd/yyyy}"><apex:param value="{!LeaseInfo.BaseLeaseEffectiveDate__c}" /></apex:outputText>
    			</apex:pageBlockSectionItem>
    			<apex:outputText value="{!LeaseInfo.RegionCD__c}"/>
    			<apex:outputText value="{!LeaseInfo.RealtySpecialist__c}"/>
    			<apex:outputText value="{!LeaseInfo.BA53User__c}"/>
		  	</apex:repeat>
		</apex:pageBlockSection>
	</apex:pageBlock>
	
