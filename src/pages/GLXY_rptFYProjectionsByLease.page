<apex:page Controller="GLXY_ProjectionsByLease" Title="Lease Projections" readonly="true" sidebar="false" cache="true">
<apex:form id="frmLease">

	<table align="center" border="0">
        <tr>
           <td align="center">
                <h1><font color="darkred" size="+2">Lease Projections</font></h1><br/>
           </td>
        </tr>
        <tr>
           <td align="center">
           		<div style="background:White; border:2px outset #888888;">
				<h2><font color="darkred" size="+1">
