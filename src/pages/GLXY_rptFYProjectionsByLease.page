			document.getElementById('{!$Component.frmLease.Total.GLXY_TOTMAY}').innerHTML = TotalAmountMAY;
			document.getElementById('{!$Component.frmLease.Total.GLXY_TOTJUN}').innerHTML = TotalAmountJUN;
			document.getElementById('{!$Component.frmLease.Total.GLXY_TOTJUL}').innerHTML = TotalAmountJUL;
