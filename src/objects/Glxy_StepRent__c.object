    <validationRules>
        <fullName>SR_Process_Date_FY</fullName>
        <active>true</active>
        <description>Process Date cannot be in a prior Fiscal Year</description>
        <errorConditionFormula>TEXT(StatusCode__c) == &apos;Estimate&apos;
&amp;&amp;
YEAR(ProcessedDate__c + 92) &lt; YEAR(TODAY() + 92)
&amp;&amp;
!CONTAINS($User.FirstName, &apos;CastIron&apos;)</errorConditionFormula>
        <errorDisplayField>ProcessedDate__c</errorDisplayField>
        <errorMessage>Process Date cannot be in a prior Fiscal Year</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SR_Process_Date_Validation</fullName>
        <active>false</active>
        <description>Process Date cannot be less than the Effective Date</description>
        <errorConditionFormula>TEXT(StatusCode__c) == &apos;Estimate&apos;
&amp;&amp;
ProcessedDate__c &lt; EffectiveDate__c</errorConditionFormula>
        <errorDisplayField>ProcessedDate__c</errorDisplayField>
        <errorMessage>Process Date cannot be less than the Effective Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SumOfNOAandIA</fullName>
        <active>false</active>
        <errorConditionFormula>IF(AND(IA_Sq_Ft__c &lt;&gt; 0,  NOA_Sq_Ft__c &lt;&gt; 0, LeaseNumber__r.BaseSquareFeet__c &lt;&gt; 0), (IA_Sq_Ft__c +  NOA_Sq_Ft__c &gt;  LeaseNumber__r.BaseSquareFeet__c), FALSE)</errorConditionFormula>
        <errorMessage>Sum of NOA and IA Sq. Ft. cannot exceed the total square feet of the Lease.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Verify_Budget_exclude_Comments</fullName>
        <active>true</active>
        <errorConditionFormula>AND(BDGFlag__c =FALSE,  ISBLANK( Budget_Exclusion_Reason__c ) )</errorConditionFormula>
        <errorDisplayField>Budget_Exclusion_Reason__c</errorDisplayField>
        <errorMessage>Please provide a reason for excluding this from the budget.</errorMessage>
    </validationRules>
