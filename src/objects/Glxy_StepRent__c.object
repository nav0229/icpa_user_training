    <fields>
        <fullName>Acceptance620Date__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Date R620 was generated and submitted to finance for processing</inlineHelpText>
        <label>620 to R7 Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>AccrualAmountCurrentFY__c</fullName>
        <externalId>false</externalId>
        <formula>If(
    (AccrualEndDate__c &gt; AccrualStartDateCY__c &amp;&amp;  Text(StatusCode__c)  == &apos;Estimate&apos; &amp;&amp;
         EffectiveDate__c &lt;= Today ()),
    ( 
      (
        (Amount__c/12) 
        * 
        ((12 * (YEAR(AccrualEndDate__c) - YEAR(AccrualStartDateCY__c))) +
          (MONTH(AccrualEndDate__c)- MONTH(AccrualStartDateCY__c)) + 1)
      )
      +
      (
        If(((YEAR(TODAY()+ 92) - Value(EffectiveDateFY__c)) == 0),
            ( 
               (DAY(DATE(YEAR(EffectiveDate__c),MONTH(EffectiveDate__c), 1))
                    - Day(EffectiveDate__c)) 
                * 
                AccrualAmountEffectiveMonthRate__c  
            ),
            0
         )
      )
    ),
   0
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Amount of transaction attributable to current FY</inlineHelpText>
        <label>Current FY Calculated Value</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>AccrualAmountEffectiveMonthRate__c</fullName>
        <defaultValue>1</defaultValue>
        <externalId>false</externalId>
        <label>Accrual Amount Effective Month Rate</label>
        <precision>16</precision>
        <required>false</required>
        <scale>4</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>AccrualAmountPriorFY__c</fullName>
        <externalId>false</externalId>
        <formula>If( (Value(EffectiveDateFY__c) &lt; YEAR(TODAY()+ 92) ),
     
     If( ((YEAR(TODAY()+ 92) - Value(EffectiveDateFY__c)) == 1),
         (
            (  ((12 * (YEAR(DATE(YEAR(EffectiveDate__c + 92),09,30)) - YEAR(EffectiveDate__c))) +
               (MONTH(DATE(YEAR(EffectiveDate__c + 92),09,30))- MONTH(EffectiveDate__c)) + 1) 
                * 
               (Amount__c/12) 
            )
            +
            (
                (DAY(DATE(YEAR(EffectiveDate__c), MONTH(EffectiveDate__c), 1))
                       - Day(EffectiveDate__c)) 
                * 
                AccrualAmountEffectiveMonthRate__c   
            )
         ),
         (Amount__c)
     ),

    0
  )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>Amount of transaction attributable to previous FY</inlineHelpText>
        <label>Prior FY Calculated Value</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>AccrualEndDate__c</fullName>
        <externalId>false</externalId>
        <formula>If(
     (If(IsNull(Acceptance620Date__c), TODAY(), Acceptance620Date__c) &gt; EffectiveDate__c),
     If(IsNull(Acceptance620Date__c), TODAY(), Acceptance620Date__c),
     EffectiveDate__c 
  )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Accrual End Date</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
        <fullName>AccrualStartDateCY__c</fullName>
        <externalId>false</externalId>
        <formula>If(EffectiveDateFY__c==Text(YEAR(TODAY()+ 92)),EffectiveDate__c,DATE((YEAR(TODAY())-1),10,01))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
