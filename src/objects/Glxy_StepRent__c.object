        <externalId>false</externalId>
        <formula>If ( ((IA_Sq_Ft__c + NOA_Sq_Ft__c) = 0),
      Amount__c,
      ((NOA_Sq_Ft__c /(IA_Sq_Ft__c + NOA_Sq_Ft__c)) * Amount__c)
)</formula>
