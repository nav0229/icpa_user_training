    <validationRules>
        <fullName>PD_Cannot_Check_Master_when_Regional</fullName>
        <active>true</active>
        <errorConditionFormula>IF( Master__c =TRUE &amp;&amp;  Cloned_from_Master__c =TRUE &amp;&amp;   ISPICKVAL(PD_Template__c , &apos;Regional&apos;) , TRUE,FALSE)</errorConditionFormula>
        <errorMessage>Please do not check Master, when PD is Regional</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PD_Checking_Is_New_Regional_PD</fullName>
        <active>true</active>
        <errorConditionFormula>IF(Master__c =FALSE &amp;&amp;  Cloned_from_Master__c =TRUE,TRUE, FALSE)  &amp;&amp;( NOT ISPICKVAL(  Service__c ,&quot;&quot;) )</errorConditionFormula>
        <errorMessage>Please do not check cloned from Master.  (OR) Please do not select Service. If you are creating  a New Regional PD</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PD_FPL_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF( Master__c == FALSE, TRUE, FALSE) &amp;&amp; ( FPL__c  = &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>FPL__c</errorDisplayField>
        <errorMessage>Please enter FPL</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PD_FPL_Validation_Cloned</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
(Master__c = TRUE),(Cloned_from_Master__c = TRUE) &amp;&amp; ( FPL__c = &quot;&quot;) 
)</errorConditionFormula>
        <errorDisplayField>FPL__c</errorDisplayField>
        <errorMessage>Please enter FPL</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PD_New_Region_PD_Number_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF(LEN (PD_Number__c) &lt;&gt; 7, TRUE, FALSE) &amp;&amp; (Master__c == FALSE)</errorConditionFormula>
        <errorDisplayField>PD_Number__c</errorDisplayField>
        <errorMessage>Please Enter 7 letter PD Number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PD_New_Region_PD_Region_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
             (Master__c == FALSE) &amp;&amp;  ( Cloned_from_Master__c  == FALSE) &amp;&amp; (ISPICKVAL( Regions__c ,&quot;&quot;) ) 
        )</errorConditionFormula>
        <errorDisplayField>Regions__c</errorDisplayField>
        <errorMessage>Please select a Region</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PD_New_Region_PD_Service_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
             ( Master__c == FALSE) &amp;&amp; ( Cloned_from_Master__c  == FALSE)&amp;&amp;(ISPICKVAL(  Service__c  ,&quot;&quot;) ) 
        )</errorConditionFormula>
        <errorDisplayField>Service__c</errorDisplayField>
        <errorMessage>Please select a Service</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PD_New_Regional_PD_Not_from_Cloned</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( 
           (Cloned_from_Master__c == TRUE) &amp;&amp; ISPICKVAL( PD_Template__c, &quot;&quot;) &amp;&amp; ( Master__c == FALSE) &amp;&amp; ISPICKVAL( PD_Template__c, &quot;Master&quot;)
         
)</errorConditionFormula>
        <errorMessage>Not a valid scenario. Please uncheck Cloned from Master</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PD_Not_Service_Validation_for_Cloned_PD</fullName>
        <active>true</active>
        <description>When creating a Cloned Master PD</description>
        <errorConditionFormula>AND(
          (Master__c = TRUE),(Cloned_from_Master__c = TRUE) &amp;&amp;  ( NOT ISPICKVAL(  Service__c  ,&quot;&quot;) )
)</errorConditionFormula>
        <errorDisplayField>Service__c</errorDisplayField>
        <errorMessage>Regional PD copied from Master, please do not  select a Service</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PD_Number_Validation_for_Cloned_PD</fullName>
        <active>true</active>
        <description>When creating a Cloned Master PD</description>
        <errorConditionFormula>AND(
          (Master__c = TRUE),(Cloned_from_Master__c = TRUE) &amp;&amp;  IF( LEN( PD_Number__c ) &lt;&gt;7, TRUE,FALSE)  
)</errorConditionFormula>
        <errorDisplayField>PD_Number__c</errorDisplayField>
        <errorMessage>Regional PD copied from Master, please enter only 7 characters in PD Number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PD_Number_Validation_for_Cloned_PD_Edit</fullName>
        <active>true</active>
        <description>When creating a Cloned Master PD</description>
        <errorConditionFormula>AND(
          (Master__c = FALSE),(Cloned_from_Master__c = TRUE) &amp;&amp;  ISPICKVAL(Regions__c, &quot;&quot;)
)</errorConditionFormula>
        <errorDisplayField>Regions__c</errorDisplayField>
        <errorMessage>Please select a Region</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PD_Number_Validation_for_Master_PD</fullName>
        <active>true</active>
        <description>When creating a New Master PD</description>
        <errorConditionFormula>AND(
          (Master__c = TRUE),(Cloned_from_Master__c = FALSE) &amp;&amp;  IF( LEN( PD_Number__c ) &lt;&gt;4, TRUE,FALSE) 
)</errorConditionFormula>
        <errorDisplayField>PD_Number__c</errorDisplayField>
        <errorMessage>For Master PD please enter only 4 characters in PD Number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PD_Region_Validation_for_Cloned_PD</fullName>
        <active>true</active>
        <description>When creating a Cloned Master PD</description>
        <errorConditionFormula>AND(
          (Master__c = TRUE),(Cloned_from_Master__c = TRUE) &amp;&amp;  ISPICKVAL( Regions__c ,&quot;&quot;) 
)</errorConditionFormula>
        <errorDisplayField>Regions__c</errorDisplayField>
        <errorMessage>Regional PD copied from Master, please select a Region</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PD_Region_Validation_for_Master_PD</fullName>
        <active>true</active>
        <description>When creating a New Master PD</description>
        <errorConditionFormula>AND(
          (Master__c = TRUE),(Cloned_from_Master__c = FALSE) &amp;&amp;  ( NOT ISPICKVAL( Regions__c ,&quot;&quot;) )  
)</errorConditionFormula>
        <errorDisplayField>Regions__c</errorDisplayField>
        <errorMessage>Please do not select a Region for Master PD</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PD_Series</fullName>
        <active>true</active>
        <errorConditionFormula>IF( LEN(Series__c)&lt;&gt; 4, TRUE, FALSE)</errorConditionFormula>
        <errorDisplayField>Series__c</errorDisplayField>
        <errorMessage>Please enter 4 digit Series</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Help_Text</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Help Text</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>var txt = &quot;What does this tool do? \n \nThe PD Library is a tool that stores position descriptions across the enterprise.  The tool allows those in HR position classification to create and maintain position descriptions, and allows read-only users, primarily managers and HR Specialists, to search for and view PDs.\n Purpose: \n This tool was developed to reduce the time spent on position classification.  The tool allows Managers and Human Resources Specialists to view position descriptions and collaborate, which in turn increases consistency and efficiency.   \n Who will use this database? \n Human Resource Specialists, Managers, and other specifically approved people.\n For information on using the tool, please consult your servicing HR office. \n For system-related issues or questions, please contact the CHRIS Help Desk at gsa.chris@gsa.gov. \n&quot;;

alert(txt);</url>
    </webLinks>
</CustomObject>
