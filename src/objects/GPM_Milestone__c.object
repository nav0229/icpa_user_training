    <fields>
        <fullName>Milestone_Progress_Flag__c</fullName>
        <externalId>false</externalId>
        <formula>IMAGE( 
CASE( Milestone_Status__c , 
&quot;Red&quot;, &quot;/resource/RedFlag&quot;, 
&quot;Yellow&quot;, &quot;/resource/YellowFlag&quot;, 
&quot;Green&quot;, &quot;/resource/GreenFlag&quot;, 
&quot;Blue&quot;, &quot;/resource/BlueFlag&quot;, 
&quot;Grey&quot;, &quot;/resource/GreyFlag&quot;,&quot;/s.gif&quot;), 
CASE( Milestone_Status__c, 
&quot;Red&quot;, &quot;Red&quot;, 
&quot;Yellow&quot;, &quot;Yellow&quot;, 
&quot;Green&quot;, &quot;Green&quot;, 
&quot;Blue&quot;, &quot;Blue&quot;, 
&quot;Grey&quot;, &quot;Grey&quot;, &quot;Milestone Progress Flag&quot;) 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
