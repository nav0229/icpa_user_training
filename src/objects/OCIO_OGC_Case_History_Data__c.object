    <validationRules>
        <fullName>Date_Closed_Mandatory_Status_Closed</fullName>
        <active>true</active>
        <description>Date Closed field is Mandatory, when Status field value is Closed. If status is &quot;Open&quot; then leave this field blank.</description>
        <errorConditionFormula>AND( ISPICKVAL (Status__c , &quot;Closed&quot;),   ISNULL(Date_Closed_Case_History_Data__c ) )||
AND (ISPICKVAL(Status__c, &quot;Open&quot;),  NOT( ISNULL(Date_Closed_Case_History_Data__c) ))</errorConditionFormula>
        <errorDisplayField>Date_Closed_Case_History_Data__c</errorDisplayField>
        <errorMessage>Date Closed field is Mandatory, when Status field value is Closed. If status is &quot;Open&quot; then leave this field blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_Closed_cannot_be_prior_to_Date_Open</fullName>
        <active>true</active>
        <description>Date Closed cannot be earlier than Date Opened</description>
        <errorConditionFormula>Date_Closed_Case_History_Data__c &lt;  Date_Opened_OGC_Case_History_Data__c</errorConditionFormula>
        <errorDisplayField>Date_Closed_Case_History_Data__c</errorDisplayField>
        <errorMessage>Date Closed cannot be earlier than Date Opened.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Estimated_Claim_Amount_greater_than_zero</fullName>
        <active>false</active>
        <description>If no claim amount is blank.</description>
        <errorConditionFormula>AND(NOT( No_Claim_Amount__c ), OR((ISNULL( Claim_Amount__c  )),  Claim_Amount__c  =0))</errorConditionFormula>
        <errorMessage>An Estimated Claim Amount must be entered or you must identify No Claim Amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NoClaimAmount_or_EstimatedClaimAmount</fullName>
        <active>false</active>
        <errorConditionFormula>AND(  No_Claim_Amount__c ,  Claim_Amount__c &lt;&gt;0)</errorConditionFormula>
        <errorDisplayField>Claim_Amount__c</errorDisplayField>
        <errorMessage>Enter a Value in &quot;Claim Amount&quot; or Check &quot;No Claim Amount&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Negative_Recovery_Amount</fullName>
        <active>true</active>
        <errorConditionFormula>Recovery_Amount__c &lt; 0</errorConditionFormula>
        <errorMessage>Recovery Amount cannot be negative.</errorMessage>
    </validationRules>
