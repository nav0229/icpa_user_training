    <validationRules>
        <fullName>Check_End_Date_and_Status_complete</fullName>
        <active>true</active>
        <errorConditionFormula>OR( 
AND( ISPICKVAL(Status__c, &quot;In Process&quot;), NOT(ISNULL(End_Date__c))), 

AND(ISBLANK(End_Date__c), NOT(ISPICKVAL(Status__c , &quot;In Process&quot;))) 
)</errorConditionFormula>
        <errorMessage>Check Status is Approved or Denied and End Date is filled in</errorMessage>
    </validationRules>
