    <validationRules>
        <fullName>Case_Cannot_Be_Closed</fullName>
        <active>true</active>
        <description>Validation rule where the case cannot be closed if the case is still in the approval process flow.</description>
        <errorConditionFormula>And(
Is_Currently_In_Approval_Approcess__c= TRUE,
ISPICKVAL(Case_Closed__c, &apos;Yes&apos;))</errorConditionFormula>
        <errorMessage>The case cannot be closed if the case is still in the approval process flow.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CloseCaseCheck_Respondent</fullName>
        <active>true</active>
        <description>Prevents changes to the Respondent record when the Case Closed checkbox is checked.</description>
        <errorConditionFormula>RecordType.Name = &apos;Suspension and Debarment Respondent&apos;
&amp;&amp; ISPICKVAL(Case_Closed__c, &apos;yes&apos;)
&amp;&amp; NOT (ISCHANGED(Case_Closed__c )) 
&amp;&amp; NOT (ISCHANGED( SD_Mark_for_Deletion__c ))
&amp;&amp; NOT (ISCHANGED( SD_RecType_Destruction_Date__c ))
&amp;&amp; NOT (ISCHANGED( SD_Destruction_Date__c ))</errorConditionFormula>
        <errorMessage>Record cannot be updated when Close Case is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CloseCaseCheck_SD</fullName>
        <active>true</active>
        <description>Prevents changes to the S&amp;D record when the Case Closed checkbox is checked.</description>
        <errorConditionFormula>RecordType.Name = &apos;Suspension and Debarment Case&apos;
&amp;&amp; ISPICKVAL(Case_Closed__c, &apos;Yes&apos;)
&amp;&amp; OR( ISCHANGED(Comments_for_SDO__c)
       , ISCHANGED(Priority_or_Easy__c) 
       , ISCHANGED(Case_Name__c) 
       , ISCHANGED(Integrity_Officer__c) 
       , ISCHANGED(SDO_Received_Date__c) 
       , ISCHANGED(Initial_SAM_Check_for_all_respondents__c) 
       , ISCHANGED(Lead_Agency_Determination_Necessary__c) 
       , ISCHANGED(Date_of_Transfer__c) 
      )</errorConditionFormula>
        <errorMessage>Record cannot be updated when Close Case is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DateofTransferCheck</fullName>
        <active>true</active>
        <description>Date of Transfer must be entered if Lead Agency is not General Services Administration</description>
        <errorConditionFormula>RecordType.DeveloperName == &apos;Suspension and Debarment Case&apos; &amp;&amp; 
( 
ISPICKVAL( Lead_Agency_Determination_Necessary__c , &apos;Yes&apos;)
  &amp;&amp; NOT ( ISPICKVAL( Lead_Agency__c , &apos;General Services Administration&apos;) ||  ISPICKVAL( Lead_Agency__c , &apos;&apos;) ) 
  &amp;&amp; ISBLANK( Date_of_Transfer__c) 
)</errorConditionFormula>
        <errorMessage>Lead Agency selected is not General Services Administration, Please populate the Date of Transfer</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DebarmentACATerminationDateCheck</fullName>
        <active>true</active>
        <description>Debarment_ACA_Termination_Date__c must not be blank if either Debarment_Decision_Date__c or ACA_Decision_Date__c are populated.</description>
        <errorConditionFormula>RecordType.DeveloperName == &apos;Suspension and Debarment Respondent&apos; &amp;&amp; 
(NOT( ISBLANK( Debarment_Decision_Date__c ))  || 
NOT( ISBLANK(  ACA_Decision_Date__c  ) ) )
&amp;&amp; ISBLANK( Debarment_ACA_Termination_Date__c )</errorConditionFormula>
        <errorMessage>Please enter the Termination Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Grounds_for_Debarment_Other</fullName>
        <active>true</active>
        <description>If Other is chosen for Grounds for Debarment, the user will be able to add addition information in this field</description>
        <errorConditionFormula>AND(
         INCLUDES(Grounds_for_Debarment__c, &quot;Other&quot;),
         ISBLANK( Grounds_for_Debarment_Other__c)
)</errorConditionFormula>
        <errorDisplayField>Grounds_for_Debarment_Other__c</errorDisplayField>
        <errorMessage>Must fill out Grounds for Debarment Other</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GroundsforDebarmentCheck</fullName>
        <active>true</active>
        <description>Grounds for Debarment must not be blank if Debarment Date is populated</description>
        <errorConditionFormula>RecordType.DeveloperName == &apos;Suspension and Debarment Respondent&apos; &amp;&amp;
NOT (ISBLANK( Debarment_Decision_Date__c )) &amp;&amp;
ISBLANK(Grounds_for_Debarment__c)</errorConditionFormula>
        <errorMessage>Please select the Grounds for Debarment value(s) when Debarment Date is entered.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>LeadAgencyActionDateCheck</fullName>
        <active>true</active>
        <description>If Lead_Agency_Action_Date is not blank, Lead_Agency_Action must not be blank and vice versa.</description>
        <errorConditionFormula>RecordType.DeveloperName == &apos;Suspension and Debarment Respondent&apos; &amp;&amp;
(
   (ISBLANK( Lead_Agency_Action__c ) &amp;&amp; NOT( ISBLANK(     Lead_Agency_Action_Date__c ) ) )
  ||
    (NOT (ISBLANK( Lead_Agency_Action__c )) &amp;&amp; ISBLANK( Lead_Agency_Action_Date__c ) )
)</errorConditionFormula>
        <errorMessage>Please populate both the Lead Agency Action and the Lead Agency Action Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MarkforDeletionCheck_Respondent</fullName>
        <active>true</active>
        <description>SD_Mark_for_Deletion__c = true
&amp;&amp; NOT ((PRIORVALUE( SD_Mark_for_Deletion__c )) = false)</description>
        <errorConditionFormula>RecordType.Name = &apos;Suspension and Debarment Respondent&apos;
&amp;&amp; SD_Mark_for_Deletion__c = true
&amp;&amp; NOT (ISCHANGED( Case_Closed__c )) 
&amp;&amp; NOT (ISCHANGED( SD_Mark_for_Deletion__c ))
&amp;&amp; NOT (ISCHANGED( SD_RecType_Destruction_Date__c ))
&amp;&amp; NOT (ISCHANGED( SD_Destruction_Date__c ))</errorConditionFormula>
        <errorMessage>Record cannot be updated when Mark for Deletion is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MarkforDeletionCheck_SD</fullName>
        <active>true</active>
        <description>RecordType.Name = &apos;Suspension and Debarment&apos;
&amp;&amp; SD_Mark_for_Deletion__c = true</description>
        <errorConditionFormula>RecordType.Name = &apos;Suspension and Debarment Case&apos;
&amp;&amp; SD_Mark_for_Deletion__c = true 
&amp;&amp; OR( ISCHANGED(Comments_for_SDO__c)
       , ISCHANGED(Priority_or_Easy__c) 
       , ISCHANGED(Case_Name__c) 
       , ISCHANGED(Integrity_Officer__c) 
       , ISCHANGED(SDO_Received_Date__c) 
       , ISCHANGED(Initial_SAM_Check_for_all_respondents__c) 
       , ISCHANGED(Lead_Agency_Determination_Necessary__c) 
       , ISCHANGED(Date_of_Transfer__c) 
      )</errorConditionFormula>
        <errorMessage>Record cannot be updated when Mark for Deletion is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SD_Assigned_Approver</fullName>
        <active>true</active>
        <description>Restricts reviewers from accessing main record when the record is not assigned.</description>
        <errorConditionFormula>NOT(
     OR( $Permission.Suspension_Debarment_Super_User_CRED 
         , $Permission.Suspension_Debarment_User_CRE 
         , AND(  $Permission.Suspension_Debarment_Approval_User_RE
                , NOT( Case_Status__c = &quot;With Integrity Officer&quot; )
                , NOT( Case_Status__c = &quot;Pending Initial IO Review&quot; )
              )
       )
   )

&amp;&amp; ISCHANGED( Priority_or_Easy__c  )</errorConditionFormula>
        <errorMessage>You must be assigned this record to edit.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SD_Case_Close_Val_Rule_1</fullName>
        <active>true</active>
        <description>Only Integrity Officers/Staff Users should be allowed to close cases, Legal or SDO users should not be able to do this.</description>
        <errorConditionFormula>AND(  ISCHANGED( Case_Closed__c ),  (  $Permission.Suspension_Debarment_Approval_User_RE   ))</errorConditionFormula>
        <errorMessage>You do not have the permissions to close or reopen this case.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SD_Case_Requirement</fullName>
        <active>true</active>
        <description>If Lead Agency is other than GSA, Lead Agency Determination Necessary and Date of Transfer should be required fields.</description>
        <errorConditionFormula>AND(
RecordType.DeveloperName = &apos;Suspension_and_Debarment_Case&apos;,
OR(
ISPICKVAL( Lead_Agency__c , &quot;Agency for International Development&quot;),
ISPICKVAL( Lead_Agency__c , &quot;Agriculture&quot;),
ISPICKVAL( Lead_Agency__c , &quot;Commerce&quot;),
ISPICKVAL( Lead_Agency__c , &quot;Defense, Air Force&quot;),
ISPICKVAL( Lead_Agency__c , &quot;Defense, Army&quot;),
ISPICKVAL( Lead_Agency__c , &quot;Defense, Defense Logistics Agency&quot;),
ISPICKVAL( Lead_Agency__c , &quot;Defense, Navy&quot;),
ISPICKVAL( Lead_Agency__c , &quot;Education&quot;),
ISPICKVAL( Lead_Agency__c , &quot;Energy&quot;),
ISPICKVAL( Lead_Agency__c , &quot;Environmental Protection Agency&quot;),
ISPICKVAL( Lead_Agency__c , &quot;Health and Human Services&quot;),
ISPICKVAL( Lead_Agency__c , &quot;Homeland Security&quot;),
ISPICKVAL( Lead_Agency__c , &quot;Homeland Security, Immigration and Customs&quot;),
ISPICKVAL( Lead_Agency__c , &quot;Enforcement&quot;),
ISPICKVAL( Lead_Agency__c , &quot;Housing and Urban Development&quot;),
ISPICKVAL( Lead_Agency__c , &quot;Interior&quot;),
ISPICKVAL( Lead_Agency__c , &quot;Justice&quot;),
ISPICKVAL( Lead_Agency__c , &quot;Labor&quot;),
ISPICKVAL( Lead_Agency__c , &quot;National Aeronautics and Space Administration&quot;),
ISPICKVAL( Lead_Agency__c , &quot;National Science Foundation&quot;),
ISPICKVAL( Lead_Agency__c , &quot;Nuclear Regulatory Commission&quot;),
ISPICKVAL( Lead_Agency__c , &quot;Office of Personnel Management&quot;),
ISPICKVAL( Lead_Agency__c , &quot;Small Business Administration&quot;),
ISPICKVAL( Lead_Agency__c , &quot;Social Security Administration&quot;),
ISPICKVAL( Lead_Agency__c , &quot;State&quot;),
ISPICKVAL( Lead_Agency__c , &quot;Transportation&quot;),
ISPICKVAL( Lead_Agency__c , &quot;Treasury&quot;),
ISPICKVAL( Lead_Agency__c , &quot;Veterans Affairs&quot;)),

ISPICKVAL(Lead_Agency_Determination_Necessary__c,&quot;&quot;),
        ISBLANK(Date_of_Transfer__c)
)</errorConditionFormula>
        <errorMessage>Lead Agency Determination Necessary and Date of Transfer is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SD_Respondent_Requirement2</fullName>
        <active>true</active>
        <description>Respondent records for which the S&amp;D case record lists a Lead Agency other than GSA, Basis, Business / Individual, and Direct / Imputed / Affiliated should not be required fields.</description>
        <errorConditionFormula>AND( 
RecordType.DeveloperName = &apos;Suspension_and_Debarment_Respondent&apos;, 
OR( 
Look_Up__r.Lead_Agency_Lookup__c= &apos;General Services Adminstration&apos;), 

ISPICKVAL(Basis__c,&quot;&quot;), 
ISPICKVAL(Business__c,&quot;&quot;),
ISPICKVAL(Direct_Imputed_Affiliated__c,&quot;&quot;) 
)</errorConditionFormula>
        <errorMessage>Basis, Business / Individual, and Direct / Imputed / Affiliated should not be required fields.</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Close_Case</fullName>
        <availability>online</availability>
        <description>Button to update Close Case and lock record.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Close Case</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/33.0/connection.js&quot;)} 

var SDC = new sforce.SObject(&quot;Suspension_and_Debarment_Case__c&quot;); 
SDC.id = &quot;{!Suspension_and_Debarment_Case__c.Id}&quot;; 

SDC.Close_Case__c = true; 
result = sforce.connection.update([SDC]); 

if ( result[0].getBoolean( &quot;success&quot; ) ) 
{ 
window.location.reload(); 
} 
else 
{ 
alert( result[0].errors.message); 
}</url>
    </webLinks>
