        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/19.0/connection.js&quot;)} 

var sr = new sforce.SObject(&quot;GLXY_Transaction__c&quot;); 
sr.id = &quot;{!Glxy_Transaction__c.Id}&quot;; 
var comments= &quot;{!JSENCODE(Glxy_Transaction__c.Rejection_Remarks__c)}&quot;;
var reason=&quot;{!Glxy_Transaction__c.Reject_Reason__c}&quot;;
var status=&quot;{!Glxy_Transaction__c.Status__c}&quot;;
var region = &quot;{!Glxy_Transaction__c.IsUserRegionMatch__c}&quot;; 
var AccrualCount = &quot;{!Glxy_Transaction__c.Count_of_Accruals__c}&quot;;

if( status==&quot;Approved by BA53&quot; || status==&quot;Rejected by BA53&quot; || status==&quot;Accepted by REXUS&quot; || region==&quot;FALSE&quot;){ 
	alert(&apos;You cannot change the status of this transaction.&apos;);
}else{
	if(comments == &quot;&quot; || reason == &quot;&quot;){
		alert(&apos;Please fill out the Rejection Remarks section and provide reason before Rejecting the record.&apos;);
	}else{
		if(reason == &quot;Failed REXUS Validation&quot;){
			alert(&apos;Please update the Rejection Reason field before Rejecting the record.&apos;);
		}else{
			if (AccrualCount != &quot;0&quot;){
				alert(&apos;Please delete the Lease Accrual(s) before Rejecting the record.&apos;);
			} else {
				sr.Status__c = &apos;Rejected by BA53&apos;; 
				sr.TransactionProcessedDate__c = new Date();
		        sr.BA53Approver__c = &quot;{!$User.Email}&quot;;
				result = sforce.connection.update([sr]); 
				window.location.reload();
			}
		}
	}
}</url>
