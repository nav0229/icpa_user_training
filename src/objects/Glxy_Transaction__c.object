    <validationRules>
        <fullName>IA_Sq_Ft_Not_Blank_on_Date_LA</fullName>
        <active>true</active>
        <description>IA Conversion Date should be NULL when IA Sq Ft is Zero</description>
        <errorConditionFormula>AND(NOT(ISBLANK(IA_NOA_Conversion_Date__c)), OR(ISBLANK(IA_RSF__c),IA_RSF__c=0), RecordType.DeveloperName =&quot;Lease_Action&quot; )</errorConditionFormula>
        <errorMessage>IA Conversion Date should be NULL when IA Sq Ft is Zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lease_Number_NOT_Null</fullName>
        <active>true</active>
        <errorConditionFormula>AND( RecordType.DeveloperName = &quot;Lease_Action&quot;,  ISBLANK(LeaseNumber__c) )</errorConditionFormula>
        <errorMessage>Lease Number Cannot be Null</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Project_Number_NOT_Null</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT(RecordType.DeveloperName = &quot;Lease_Action&quot;), ISBLANK( Project_Number__c ) )</errorConditionFormula>
        <errorMessage>Project Number Cannot be NULL</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RequiredRegion</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Region__c,&quot;&quot;)</errorConditionFormula>
        <errorMessage>Region cannot be left blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TransactionIDChanged</fullName>
        <active>true</active>
        <errorConditionFormula>ISCHANGED(Name)</errorConditionFormula>
        <errorMessage>Cannot Change Transaction ID</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Verify_Estimated_IA_Amount</fullName>
        <active>true</active>
        <description>Estimated IA amount  is greater than or equal to zero and is less than or equal to Estimated Annual Rent</description>
        <errorConditionFormula>AND(NOT( RecordType.DeveloperName =&quot;Lease_Action&quot;), OR(EstimatedIAAmount__c &gt; EstimatedAnnualAmount__c,EstimatedIAAmount__c &lt; 0 ) )</errorConditionFormula>
        <errorMessage>Estimated IA Rent cannot be greater than Estimated Annual Amount or Less than Zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Verify_IANOA_Conversion_date_1</fullName>
        <active>true</active>
        <errorConditionFormula>AND(IA_RSF__c &gt;0, OR(MONTH(IA_NOA_Conversion_Date__c) &lt;&gt;10,DAY(IA_NOA_Conversion_Date__c)&lt;&gt;1,ISNULL(IA_NOA_Conversion_Date__c )), (RecordType.DeveloperName = &quot;Lease_Action&quot;))</errorConditionFormula>
        <errorMessage>IA/NOA Conversion Date must be filled in if IA RSF is greater than 0 and Date  must be Oct 1.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Verify_IANOA_Conversion_date_2</fullName>
        <active>true</active>
        <description>IA/NOA Conversion Date must be as of 10/1 and must be greater than Max effective date of All Digests.</description>
        <errorConditionFormula>AND(  NOT(ISNULL(IA_NOA_Conversion_Date__c )), IA_RSF__c &gt;0, IA_NOA_Conversion_Date__c &lt; Digest_Effective_Date__c,  RecordType.DeveloperName = &quot;Lease_Action&quot;)</errorConditionFormula>
        <errorMessage>IA/NOA Conversion Date must be as of 10/1 and must be greater than Max effective date of All Digests.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Verify_IA_RSF_is_valid</fullName>
        <active>true</active>
        <description>Checks that the IA RSF is less than max and greater than zero</description>
        <errorConditionFormula>AND(OR(IA_RSF__c&lt;0,IA_RSF__c &gt;  Lease_Agreement_RSF__c ),$User.Alias &lt;&gt;&quot;castiron&quot;)</errorConditionFormula>
        <errorMessage>IA RSF must be greater than 0 but less than the Maximum Agreement value from the digests.</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Approve</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Approve</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/19.0/connection.js&quot;)} 

var sr = new sforce.SObject(&quot;GLXY_Transaction__c&quot;); 
sr.id = &quot;{!Glxy_Transaction__c.Id}&quot;; 
var count1=&quot;{!Glxy_Transaction__c.Count_of_digests_w_Accrual__c}&quot;;
var count2=&quot;{!Glxy_Transaction__c.Count_of_Accruals__c}&quot;;
var reason1=&quot;{!Glxy_Transaction__c.Reject_Reason__c}&quot;; 
var reason2=&quot;{!JSENCODE(Glxy_Transaction__c.Rejection_Remarks__c)}&quot;;
var RT=&quot;{!Glxy_Transaction__c.Record_Type_Lookup__c}&quot;;
var status=&quot;{!Glxy_Transaction__c.Status__c}&quot;;
var iadate = &quot;{!TEXT(Glxy_Transaction__c.IA_NOA_Conversion_Date__c)}&quot;;
var iad = &quot;{!DATEVALUE(TEXT(Glxy_Transaction__c.IA_NOA_Conversion_Date__c))}&quot;;
var NewLeaseNumber = &quot;{!Glxy_Transaction__c.New_Lease_Number__c}&quot;;
var LeaseAction = &quot;{!Glxy_Transaction__c.Lease_Action__c}&quot;;

if (iadate != &apos;&apos;){
	iadate=new Date(&quot;{!TEXT(Glxy_Transaction__c.IA_NOA_Conversion_Date__c)}&quot;);
}
if (iad != &apos;&apos;){
	iad = new Date(&quot;{!DATEVALUE(TEXT(Glxy_Transaction__c.IA_NOA_Conversion_Date__c))}&quot;);
}

var date=&quot;{!DAY(Glxy_Transaction__c.IA_NOA_Conversion_Date__c)}&quot;;
var month=&quot;{!MONTH(Glxy_Transaction__c.IA_NOA_Conversion_Date__c)}&quot;;
var tiarsf =&quot;{!Glxy_Transaction__c.IA_RSF_RLLUP__c}&quot;.valueOf();
var region = &quot;{!Glxy_Transaction__c.IsUserRegionMatch__c}&quot;;
var user = &quot;{!$User.Alias}&quot;;
var rsf = &quot;{!Glxy_Transaction__c.IA_RSF__c}&quot;.valueOf();
var effdate = &quot;{!DATEVALUE(TEXT(Glxy_Transaction__c.Digest_Effective_Date__c))}&quot;;

if (effdate != &apos;&apos;){
	effdate = new Date(&quot;{!DATEVALUE(TEXT(Glxy_Transaction__c.Digest_Effective_Date__c))}&quot;);
}

if(status==&quot;Approved by BA53&quot; || status==&quot;Rejected by BA53&quot; || status==&quot;Accepted by REXUS&quot; || region==&quot;FALSE&quot;){
	alert(&apos;You cannot change the status of this transaction.&apos;);
}else{
	
	if(RT==&quot;Lease Action&quot;){
        
        //alert(&apos;iad=&apos; + iad); alert(&apos;effdate=&apos; + effdate); alert(rsf); alert(date); alert(month);
        //alert(&apos;iad &lt; effdate&apos; + (iad &lt; effdate));
        
        if(rsf != 0 &amp;&amp; ((date != 1 || month != 10) || iad==&apos;&apos; || iad &lt; effdate)){
		//if(rsf &gt; 0 &amp;&amp; !(iad &lt; effdate || iadate == &apos;&apos; || date != 1 || month != 10)){
			alert(&apos;When IA Sq ft is greater than 0, the IA/NOA Conversion Date must be 10/1, and must be greater than Max effective date of All Digests.&apos;);
		}else{
			if(rsf == &apos;0&apos; &amp;&amp; iadate != &apos;&apos;){
				alert(&apos;IA Conversion Date should be NULL when IA Sq Ft is Zero&apos;);
			} else {
				if(count1!=count2){
					alert(&apos;One or more of the lease digests is missing accruals, please fix.&apos;);
				}else{
					if(reason1!= &quot;&quot; || reason2!= &quot;&quot;){
						alert(&apos;You cannot have a value on Rejection Reason or Remarks on Approval.&apos;);
					} else {
						sr.Status__c = &apos;Approved by BA53&apos;; 
						sr.TransactionProcessedDate__c = new Date();
						sr.BA53Approver__c=&quot;{!$User.Email}&quot;;
						result = sforce.connection.update([sr]); 
						window.location.reload();
					}
				}
			}
		}	
	
	}else{
	
		//alert(RT);alert(tiarsf); alert(date); alert(month); alert(iadate); 
	    if(tiarsf != 0 &amp;&amp; ((date != 1 || month != 10) || iadate==&quot;&quot;)){
        	alert(&apos;IA/NOA Conversion Date must be filled in if IA RSF is greater than 0 and Date must be Oct 1.&apos;);
       	}else{
			if(tiarsf == &apos;0&apos; &amp;&amp; (iadate != &quot;&quot;)){
				alert(&apos;IA Conversion Date should be NULL when Total IA Sq Ft is Zero&apos;);
			}else{
				if(reason1!=&quot;&quot; || reason2!=&quot;&quot;){
					alert(&apos;You cannot have a value on Rejection Reason or Remarks on Approval.&apos;);
				}else{
					if((RT == &quot;Project Final Certification&quot;) &amp;&amp; (NewLeaseNumber.length == 0)){
						if(LeaseAction == &quot;New&quot; || LeaseAction == &quot;Superseding&quot;  || LeaseAction == &quot;New/Replacing&quot; || LeaseAction == &quot;Succeeding&quot;){
					 		alert(&apos;New Lease Number cannot have a NULL value&apos;);
					 	}else{
						 	sr.Status__c = &apos;Approved by BA53&apos;; 
							sr.TransactionProcessedDate__c=new Date();//System.Now();
							sr.BA53Approver__c=&quot;{!$User.Email}&quot;;
							result = sforce.connection.update([sr]); 
							window.location.reload();
						}	
					} else {
						sr.Status__c = &apos;Approved by BA53&apos;; 
						sr.TransactionProcessedDate__c=new Date();//System.Now();
						sr.BA53Approver__c=&quot;{!$User.Email}&quot;;
						result = sforce.connection.update([sr]); 
						window.location.reload();
					}
				}
			}
		}
	}
	
}</url>
