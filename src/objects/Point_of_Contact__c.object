    <validationRules>
        <fullName>Point_Of_Contact_Type_One</fullName>
        <active>true</active>
        <description>&quot;Point of Contact Type&quot; and &quot;Assignee&quot; fields cannot be filled together.</description>
        <errorConditionFormula>AND(
NOT( ISPICKVAL( Point_of_Contact_Type__c ,&quot;&quot;) ), 
Assignee__c = TRUE)</errorConditionFormula>
        <errorDisplayField>Assignee__c</errorDisplayField>
        <errorMessage>&quot;Point of Contact Type&quot; and &quot;Assignee&quot; fields cannot be filled together.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Point_Of_Contact_Type_Or_Assignee</fullName>
        <active>true</active>
        <description>Either &quot;Point of Contact Type&quot; field Or &quot;Assignee&quot; field should be filled.</description>
        <errorConditionFormula>AND(
ISPICKVAL( Point_of_Contact_Type__c ,&quot;&quot;) , 
Assignee__c = FALSE )</errorConditionFormula>
        <errorDisplayField>Point_of_Contact_Type__c</errorDisplayField>
        <errorMessage>Either &quot;Point of Contact Type&quot; field Or &quot;Assignee&quot; field should be filled.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Point_of_Contact_Type_Principal</fullName>
        <active>true</active>
        <description>&quot;Point of Contact Type&quot; field value &quot;Principal&quot; is only available for &quot;Media Affairs &amp; Public Engagement&quot; record.</description>
        <errorConditionFormula>AND( OCM_Tracker_Identifier__r.RecordType.DeveloperName &lt;&gt; &quot;Media_Affairs_Public_Engagement&quot;,  ISPICKVAL(Point_of_Contact_Type__c , &quot;Principal&quot;))</errorConditionFormula>
        <errorDisplayField>Point_of_Contact_Type__c</errorDisplayField>
        <errorMessage>&quot;Point of Contact Type&quot; field value &quot;Principal&quot; is only available for &quot;Media Affairs &amp; Public Engagement&quot; record.</errorMessage>
    </validationRules>
