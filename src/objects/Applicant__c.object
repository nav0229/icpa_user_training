    <fields>
        <fullName>Gross_Annual_Income__c</fullName>
        <externalId>false</externalId>
        <formula>IF( ISPICKVAL( Stated_Gross_Income_Frequency__c , &quot;Weekly&quot;) , ( Stated_Gross_Income__c * 52),
 (IF(ISPICKVAL( Stated_Gross_Income_Frequency__c , &quot;Bi-Weekly&quot;) ,(Stated_Gross_Income__c * 26),
    ( IF(ISPICKVAL( Stated_Gross_Income_Frequency__c , &quot;Monthly&quot;), (Stated_Gross_Income__c * 12) ,
        (IF(ISPICKVAL( Stated_Gross_Income_Frequency__c , &quot;Yearly&quot;), Stated_Gross_Income__c,
          (IF(ISPICKVAL( Stated_Gross_Income_Frequency__c , &quot;Semi-monthly&quot;) ,(Stated_Gross_Income__c * 24),
             (IF(ISPICKVAL( Stated_Gross_Income_Frequency__c , &quot;Hourly&quot;) ,(Stated_Gross_Income__c * 2080),
               (IF(ISPICKVAL( Stated_Gross_Income_Frequency__c , &quot;Quarterly&quot;) ,(Stated_Gross_Income__c * 4),
                (IF(ISPICKVAL( Stated_Gross_Income_Frequency__c , &quot;Semi-annual&quot;) ,(Stated_Gross_Income__c * 2),0 )))))))))))))))
+
IF( ISPICKVAL(  Other_Income_Source_Frequency__c  , &quot;Weekly&quot;) , (  Other_Income_Amount__c  * 52),
 (IF(ISPICKVAL(  Other_Income_Source_Frequency__c , &quot;Bi-Weekly&quot;) ,( Other_Income_Amount__c  * 26),
    ( IF(ISPICKVAL(  Other_Income_Source_Frequency__c  , &quot;Monthly&quot;), ( Other_Income_Amount__c  * 12) ,
        (IF(ISPICKVAL(  Other_Income_Source_Frequency__c  , &quot;Yearly&quot;),  Other_Income_Amount__c ,
          (IF(ISPICKVAL(  Other_Income_Source_Frequency__c  , &quot;Semi-monthly&quot;) ,( Other_Income_Amount__c  * 24),
             (IF(ISPICKVAL(  Other_Income_Source_Frequency__c  , &quot;Hourly&quot;) ,( Other_Income_Amount__c  * 2080),
               (IF(ISPICKVAL(  Other_Income_Source_Frequency__c  , &quot;Quarterly&quot;) ,( Other_Income_Amount__c  * 4),
                (IF(ISPICKVAL(  Other_Income_Source_Frequency__c  , &quot;Semi-annual&quot;) ,( Other_Income_Amount__c  * 2),0 )))))))))))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Gross Annual Income</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Gross_Monthly_Income2__c</fullName>
        <externalId>false</externalId>
        <formula>Gross_Annual_Income__c /12</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Gross Monthly Income</label>
        <precision>18</precision>
