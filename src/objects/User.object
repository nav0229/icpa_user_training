    <validationRules>
        <fullName>Cell_cannot_be_edited</fullName>
        <active>true</active>
        <description>Prevent users from editing this field. Only Administrators are allowed to edit this field.</description>
        <errorConditionFormula>AND(ISCHANGED( MobilePhone ), NOT(OR($Profile.Name = &quot;GSA System Administrator&quot;,$Profile.Name = &quot;System Administrator&quot;,$Profile.Name = &quot;GSA System User&quot;)))</errorConditionFormula>
        <errorDisplayField>MobilePhone</errorDisplayField>
        <errorMessage>Please access your Self-Service profile in GCIMS to update this field.  Your Mobile number will be updated in Salesforce shortly.</errorMessage>
