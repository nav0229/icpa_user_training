    <validationRules>
        <fullName>Invalid_Reason_Code</fullName>
        <active>false</active>
        <description>If Document Status = Invalid, then Invalid Reason Code must be entered</description>
        <errorConditionFormula>ISPICKVAL(Document_Status__c, &quot;Invalid&quot;) &amp;&amp; ISPICKVAL(Invalid_Reason_Code__c,&quot;&quot;)</errorConditionFormula>
        <errorDisplayField>Invalid_Reason_Code__c</errorDisplayField>
        <errorMessage>You must enter an Invalid Reason Code for Documents labeled Invalid</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_Invalid_Reason_Description</fullName>
        <active>true</active>
        <description>If Invalid Reason Code = Other, then a description must be provided</description>
        <errorConditionFormula>ISPICKVAL(Invalid_Reason_Code__c, &quot;Other&quot;) &amp;&amp; ISBLANK( Other_Reason_Code_Description__c )</errorConditionFormula>
        <errorDisplayField>Other_Reason_Code_Description__c</errorDisplayField>
        <errorMessage>If the invalid reason code is Other, then you must enter a description</errorMessage>
    </validationRules>
