    <validationRules>
        <fullName>Manual_Loan_Conditions</fullName>
        <active>false</active>
        <description>Validation rule ensures the Loan Condition is NOT changed if the Loan Condition is Automatic. If the loan condition is Manual, THEN the Loan Condition Type can be edited.</description>
        <errorConditionFormula>Generated_Type__c = &quot;Auto&quot; &amp;&amp;  ISCHANGED(Master_Checklist__c )</errorConditionFormula>
        <errorDisplayField>Master_Checklist__c</errorDisplayField>
        <errorMessage>You cannot edit Loan Conditions that have been assigned automatically to the Application</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Property_Loan_Condition</fullName>
        <active>true</active>
        <description>Rule ensure that Loan Condition that are Type = Property have a Property associated to it.</description>
        <errorConditionFormula>Loan_Condition_Type__c = &quot;Property&quot; &amp;&amp;  ISBLANK(Property__c )</errorConditionFormula>
        <errorDisplayField>Property__c</errorDisplayField>
        <errorMessage>For Property Loan Conditions, you must have a Property associated to the record</errorMessage>
    </validationRules>
