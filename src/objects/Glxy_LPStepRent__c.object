    <fields>
        <fullName>Annual_Obligation_IA__c</fullName>
        <externalId>false</externalId>
        <formula>(
  IA_Sq_Ft__c /
  IF(
    ((IA_Sq_Ft__c + NOA_Sq_Ft__c) &lt;&gt; 0),
    (IA_Sq_Ft__c + NOA_Sq_Ft__c),
    1 
  )
) 

* Amount__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Annual Obligation IA</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Annual_Obligation_NOA__c</fullName>
        <externalId>false</externalId>
        <formula>If ( ((IA_Sq_Ft__c + NOA_Sq_Ft__c) = 0),
      Amount__c,
      ((NOA_Sq_Ft__c /(IA_Sq_Ft__c + NOA_Sq_Ft__c)) * Amount__c)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Annual Obligation NOA</label>
        <precision>18</precision>
        <required>false</required>
