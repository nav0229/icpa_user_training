    <validationRules>
        <fullName>SumOfNOAandIA</fullName>
        <active>true</active>
        <description>Sum of NOA and IA Sq. Ft. cannot exceed the total square feet of the Lease Project</description>
        <errorConditionFormula>IF(AND(CONTAINS($User.FirstName, &apos;CastIron&apos;), ProjectNumber__r.SquareFeet__c  &lt;&gt; 0), (IA_Sq_Ft__c + NOA_Sq_Ft__c &gt; ProjectNumber__r.SquareFeet__c), FALSE)</errorConditionFormula>
        <errorDisplayField>NOA_Sq_Ft__c</errorDisplayField>
        <errorMessage>Sum of NOA and IA Sq. Ft. cannot exceed the total square feet of the Lease Project</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Verify_Budget_exclude_Comments</fullName>
        <active>true</active>
        <errorConditionFormula>AND(BDGFlag__c =FALSE,  ISBLANK( Budget_Exclusion_Reason__c ) )</errorConditionFormula>
        <errorDisplayField>Budget_Exclusion_Reason__c</errorDisplayField>
