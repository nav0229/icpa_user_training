    <validationRules>
        <fullName>FedRAMP_Cost_Savings_needs_PATO_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT(ISNULL( Package_Cost_Savings__c )), ISNULL(PATO_Approved_Date__c) )</errorConditionFormula>
        <errorMessage>Please enter P-ATO Approved Date before adding Package Cost Savings</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FedRAMP_Reason_Removed_Susp_required</fullName>
        <active>true</active>
        <description>Reason Removed or Suspended is required when package is Removed, ATO Cancelled, or Package Exception.</description>
        <errorConditionFormula>AND(
OR(ISPICKVAL(Package_Status__c , &quot;Removed&quot;), ISPICKVAL(Package_Status__c , &quot;ATO Cancelled&quot;), ISPICKVAL(Package_Status__c , &quot;Package Exception&quot;)),
ISBLANK( Denied_Removed_Reason__c ) )</errorConditionFormula>
        <errorMessage>Reason Removed or Suspended is required when Package Status is Removed, ATO Cancelled, or Package Exception</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FedRAMP_SSP_Score_only_w_Submitted_Docs</fullName>
        <active>true</active>
        <description>Preliminary SSP Score can only be set if Submitted Documents are YES</description>
        <errorConditionFormula>AND( ISPICKVAL( Review_Category__c  , &quot;JAB&quot;), 
NOT(ISPICKVAL(  Submitted_Documents__c , &quot;Yes&quot;)), 
NOT(ISPICKVAL( Prelimnary_SSP_Readiness_Rating__c, &quot;&quot; )) 
)</errorConditionFormula>
        <errorDisplayField>Submitted_Documents__c</errorDisplayField>
        <errorMessage>Please check for Submitted Documents before assigning a Preliminary SSP Score to the package</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>FedRAMP_needs_PATO_Date_to_Authorize</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL( Review_Category__c  , &quot;JAB&quot;), ISPICKVAL( Package_Status__c , &quot;Authorized&quot;),   ISNULL(PATO_Approved_Date__c) )</errorConditionFormula>
        <errorMessage>Please enter P-ATO Approved Date to Authorize this package</errorMessage>
    </validationRules>
</CustomObject>
