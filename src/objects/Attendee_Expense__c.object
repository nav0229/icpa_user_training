    <validationRules>
        <fullName>Estimated_Expenses_cannot_be_less_than_0</fullName>
        <active>true</active>
        <description>Will not allow a zero or negative value in the Estimated Amount field</description>
        <errorConditionFormula>Estimated_Amount__c  &lt; 0</errorConditionFormula>
        <errorMessage>Your Estimated Amount cannot be a negative amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Expense_category_equals_other</fullName>
        <active>true</active>
        <description>For Event Approval Conference Attendee add on. This rule will make the &quot;Description of Other&quot; field required if the expense category = Other</description>
        <errorConditionFormula>AND(ISPICKVAL(  Expense_Category__c  , &quot;Other&quot;), 
ISBLANK( Description_of_Other__c ) )</errorConditionFormula>
        <errorDisplayField>Description_of_Other__c</errorDisplayField>
        <errorMessage>You must provide a description of why &quot;Other&quot; was chosen</errorMessage>
    </validationRules>
