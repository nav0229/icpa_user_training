        <active>true</active>
        <description>The Loan Officer or Team Lead must have an access to manually turn the result of “Fail” to “Mitigate” while conducting
underwriting analysis</description>
        <errorConditionFormula>AND(OR($Profile.Name = &quot;AP Team Lead&quot;, $Profile.Name = &quot;AP Loan Officer&quot;),
OR( 
ISCHANGED(Condo_HOA_fee_not_listed_on_Application__c ) &amp;&amp; ISPICKVAL(PRIORVALUE(Condo_HOA_fee_not_listed_on_Application__c ),&quot;Fail&quot;) &amp;&amp; NOT(ISPICKVAL(Condo_HOA_fee_not_listed_on_Application__c,&quot;Mitigated&quot;)), 
ISCHANGED(Grants_not_listed_on_application__c ) &amp;&amp; ISPICKVAL(PRIORVALUE(Grants_not_listed_on_application__c ),&quot;Fail&quot;) &amp;&amp; NOT(ISPICKVAL(Grants_not_listed_on_application__c ,&quot;Mitigated&quot;)), 
ISCHANGED(IRS_Transcript_s_Received__c ) &amp;&amp; ISPICKVAL(PRIORVALUE(IRS_Transcript_s_Received__c ),&quot;Fail&quot;) &amp;&amp; NOT(ISPICKVAL(IRS_Transcript_s_Received__c ,&quot;Mitigated&quot;)), 
ISCHANGED(Not_an_SBA_Employee__c ) &amp;&amp; ISPICKVAL(PRIORVALUE(Not_an_SBA_Employee__c ),&quot;Fail&quot;) &amp;&amp; NOT(ISPICKVAL(Not_an_SBA_Employee__c ,&quot;Mitigated&quot;)), 

ISCHANGED(U_S_Citizen__c ) &amp;&amp; ISPICKVAL(PRIORVALUE(U_S_Citizen__c ),&quot;Fail&quot;) &amp;&amp; NOT(ISPICKVAL(U_S_Citizen__c ,&quot;Mitigated&quot;)), 
ISCHANGED(No_Character_Issues__c ) &amp;&amp; ISPICKVAL(PRIORVALUE(No_Character_Issues__c ),&quot;Fail&quot;) &amp;&amp; NOT(ISPICKVAL(No_Character_Issues__c ,&quot;Mitigated&quot;)), 

ISCHANGED(Damaged_Property_Owned_by_Applicants__c ) &amp;&amp; ISPICKVAL(PRIORVALUE(Damaged_Property_Owned_by_Applicants__c ),&quot;Fail&quot;) &amp;&amp; NOT(ISPICKVAL(Damaged_Property_Owned_by_Applicants__c ,&quot;Mitigated&quot;)), 
ISCHANGED(Primary_Residence__c ) &amp;&amp; ISPICKVAL(PRIORVALUE(Primary_Residence__c ),&quot;Fail&quot;) &amp;&amp; NOT(ISPICKVAL(Primary_Residence__c ,&quot;Pass&quot;)), 
ISCHANGED(Damaged_Address_IRS_CBR_Match__c ) &amp;&amp; ISPICKVAL(PRIORVALUE(Damaged_Address_IRS_CBR_Match__c ),&quot;Fail&quot;) &amp;&amp; NOT(ISPICKVAL(Damaged_Address_IRS_CBR_Match__c ,&quot;Mitigated&quot;)), 
