ISCHANGED(Damaged_Property_Type_Eligibility__c) &amp;&amp; ISPICKVAL(PRIORVALUE( Damaged_Property_Type_Eligibility__c),&quot;Fail&quot;) &amp;&amp; NOT(ISPICKVAL( Damaged_Property_Type_Eligibility__c,&quot;Mitigated&quot;)) 
