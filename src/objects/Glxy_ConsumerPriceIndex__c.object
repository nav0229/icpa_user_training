    <fields>
        <fullName>Annual_Obligation_IA__c</fullName>
        <externalId>false</externalId>
        <formula>(
  IA_Sq_Ft__c /
  IF(
    ((IA_Sq_Ft__c + NOA_Sq_Ft__c) &lt;&gt; 0),
    (IA_Sq_Ft__c + NOA_Sq_Ft__c),
    1 
  )
) 

* AnnualAmount__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Annual Obligation IA</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <type>Currency</type>
    </fields>
    <fields>
