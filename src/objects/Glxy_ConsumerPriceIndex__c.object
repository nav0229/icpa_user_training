        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>PastDueIndicator__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
(
    AND(
          (Text(StatusCD__c)  == &apos;Estimate&apos;),
          (LeaseNumber__r.HoldoverStatusInd__c = False), 
          (BDGFlag__c == true), 
          (
              (MONTH(Today()) + 12 - MONTH(EffectiveDate__c) +
              12 * (YEAR(Today()) - YEAR(EffectiveDate__c) - 1)) &gt; 1
          )
       )
)
,
true, false)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Past Due Indicator</label>
