        <externalId>false</externalId>
        <formula>IF( ISBLANK( EndDate__c ), TODAY() - StartDate__c , EndDate__c - StartDate__c )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Age (Days)</label>
