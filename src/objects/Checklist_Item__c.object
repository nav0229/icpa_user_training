    <validationRules>
        <fullName>Invalid_Reason_Code</fullName>
        <active>true</active>
        <description>Validation rule ensure that a Invalid Reason Code is entered when the Checklist Item Status is set to Invalid</description>
        <errorConditionFormula>ISPICKVAL( Checklist_Item_Status__c , &quot;Invalid&quot;) &amp;&amp; ISPICKVAL( Invalid_Reason_Code__c,&quot;&quot;) &amp;&amp; ISPICKVAL(  Type__c , &quot;Document Gathering&quot;)</errorConditionFormula>
        <errorDisplayField>Invalid_Reason_Code__c</errorDisplayField>
        <errorMessage>You must enter an Invalid Reason Code for Documents labeled Invalid</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_Invalid_Reason_Description</fullName>
        <active>true</active>
        <description>If Invalid Reason Code = Other, then a description must be provided</description>
        <errorConditionFormula>(ISPICKVAL( Invalid_Reason_Code__c , &quot;Other&quot;) &amp;&amp; ISBLANK(  Other_Reason_Code_Description__c)&amp;&amp;  ISPICKVAL( Type__c , &quot;Document Gathering&quot;) )

|| 

(ISPICKVAL( Checklist_Item_Status__c , &quot;Invalid&quot;) &amp;&amp;  ISPICKVAL( Type__c , &quot;Closing Documents&quot;)&amp;&amp; ISBLANK(  Other_Reason_Code_Description__c))</errorConditionFormula>
        <errorDisplayField>Other_Reason_Code_Description__c</errorDisplayField>
        <errorMessage>You must enter an Invalid Reason Description for Documents labeled Invalid</errorMessage>
    </validationRules>
