    <validationRules>
        <fullName>Check_End_Date_on_Activity</fullName>
        <active>true</active>
        <errorConditionFormula>AND( $RecordType.DeveloperName == &quot;Stage_Activities&quot;, ISBLANK( End_Date__c ), ISPICKVAL( Status__c , &quot;Completed&quot;))</errorConditionFormula>
        <errorMessage>End Date must be filled in and Status set to “Complete” to save this record</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Status_on_Activity</fullName>
        <active>true</active>
        <errorConditionFormula>AND( $RecordType.DeveloperName == &quot;Stage_Activities&quot;, NOT(ISBLANK(End_Date__c)), NOT(ISPICKVAL(Status__c , &quot;Completed&quot;)))</errorConditionFormula>
        <errorMessage>End Date must be filled in and Status set to “Complete” to save this record</errorMessage>
    </validationRules>
