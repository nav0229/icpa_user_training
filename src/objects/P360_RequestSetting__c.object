    <fields>
        <fullName>MaxScore__c</fullName>
        <description>The total maximum points possible for a &apos;new application&apos; request</description>
        <externalId>false</externalId>
        <formula>Q1_Score__c 
+ Q2_Score__c 
+ Q3_Score__c 
+ ( MAX ( Q4A_Score__c, Q4B_Score__c, Q4C_Score__c ) )
+ ( 5 * Q5_Score__c) /* 5 implies the max. number of multi-select options that can be selected for &apos;type of users are impacted&apos; field on the request object. If those picklist options are increased or decreased, update that number accordingly here */
+ ( 6 * Q6_Score__c) /* 6 implies the max. number of multi-select options that can be selected for &apos;benefits of implementation&apos; field on the request object. If those picklist options are increased or decreased, update that number accordingly here */</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>The total maximum points possible for a &apos;new application&apos; request</inlineHelpText>
        <label>Max Score</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
