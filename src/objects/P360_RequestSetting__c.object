    <validationRules>
        <fullName>P360_UsersCannotChangeName</fullName>
        <active>true</active>
        <description>P360 - No one can change the Name because it is being used in the Process Builder to update all request records with the score configuration record whose name is &apos;Request&apos;</description>
        <errorConditionFormula>ISCHANGED( Name )</errorConditionFormula>
        <errorMessage>You do not have sufficient privileges to perform this action</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>P360_UsersCannotCreateRecords</fullName>
        <active>true</active>
        <description>P360 - Only System Administrators can create records for this object</description>
        <errorConditionFormula>AND ( ISNEW() , 
 $Profile.Name  &lt;&gt; &apos;GSA System Administrator&apos; , 
 $Profile.Name  &lt;&gt; &apos;System Administrator&apos; , 
 $Profile.Name  &lt;&gt; &apos;GSA System User&apos;
)</errorConditionFormula>
        <errorMessage>You do not have sufficient privileges to create new score configurations</errorMessage>
    </validationRules>
