    <validationRules>
        <fullName>IA_RSF_not_Negative</fullName>
        <active>true</active>
        <errorConditionFormula>IASquareFeet__c  &lt; 0</errorConditionFormula>
        <errorMessage>IA Square Feet cannot be a Negative Value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IA_RSF_not_greater_than_New_RSF</fullName>
        <active>true</active>
        <errorConditionFormula>IASquareFeet__c  &gt;  Total_RSF__c</errorConditionFormula>
        <errorMessage>IA  Square Feet cannot be greater than New Square Feet</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Released_RSF_Not_Change_on_NULL</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCHANGED(RSF_Released__c),ISNULL(PRIORVALUE(RSF_Released__c)), $User.Alias &lt;&gt; &quot;castiron&quot;)</errorConditionFormula>
        <errorMessage>RSF Released Cannot Be changed when NULL</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Verify_RSF_Released_Not_Negative</fullName>
        <active>true</active>
        <description>Checks that value in RSF Released is less than zero.</description>
        <errorConditionFormula>AND(ISCHANGED(RSF_Released__c),RSF_Released__c&lt;0 ,$User.Alias &lt;&gt;&quot;castiron&quot;)</errorConditionFormula>
        <errorMessage>Value for RSF Released cannot be Negative.</errorMessage>
    </validationRules>
