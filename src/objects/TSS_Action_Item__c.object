    <validationRules>
        <fullName>Actual_Completion_can_not_be_future</fullName>
        <active>true</active>
        <description>Actual completion date can not be further than 2 days out</description>
        <errorConditionFormula>IF( Actual_Completion_Date__c  &gt;  (TODAY() +2), TRUE, FALSE)</errorConditionFormula>
        <errorDisplayField>Actual_Completion_Date__c</errorDisplayField>
        <errorMessage>Actual Completion Date can be no more than two days in the future. Please choose a different date or update this date after the Actual Completion Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TSS_Action_Item_Deferred_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND (Item_Deferred__c, OR (ISBLANK(Deferred_Completion_Date__c), ISBLANK(Action_Item_Deferred_Justification__c  )))</errorConditionFormula>
        <errorDisplayField>Item_Deferred__c</errorDisplayField>
        <errorMessage>Please provide justification and completion date if action item is deferred.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TSS_Action_Item_Name_length_limitation</fullName>
        <active>true</active>
        <errorConditionFormula>IF(LEN( Name ) &gt; 40, true, false)</errorConditionFormula>
        <errorMessage>The Action Item name must be less than 40 characters</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TSS_Completed_Date_is_required</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ISCHANGED(Status__c), AND(ISBLANK(Actual_Completion_Date__c ), ISPICKVAL(Status__c,&apos;Completed&apos;)), false)</errorConditionFormula>
        <errorDisplayField>Actual_Completion_Date__c</errorDisplayField>
        <errorMessage>Actual Completed Date is required to complete an Action Item.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TSS_Est_Completion_Date_Validation</fullName>
        <active>true</active>
        <description>As of 10/10/2017, the FY17 TSS action plans, to include all action items must be completed on or before 06/01/2018 per Rob Lacey (case# 31243)</description>
        <errorConditionFormula>If( Estimated_Completion_Date__c  &gt; DATE(2018, 6, 1), True, false)</errorConditionFormula>
        <errorDisplayField>Estimated_Completion_Date__c</errorDisplayField>
        <errorMessage>Action Items MUST be completed on or before 06/01/2018</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TSS_Estimated_date_must_be_future</fullName>
        <active>true</active>
        <description>Must be a future date</description>
        <errorConditionFormula>OR(AND(ISNEW(), Estimated_Completion_Date__c &lt; TODAY()), AND(ISCHANGED(Estimated_Completion_Date__c), Estimated_Completion_Date__c &lt; DATEVALUE(CreatedDate)))</errorConditionFormula>
        <errorMessage>Estimated Completion Date must be a future date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TSS_Explan_for_Cancellation_is_required</fullName>
        <active>true</active>
        <description>Explanation for Cancellation is required when we select the status as &apos;Cancelled&apos;</description>
        <errorConditionFormula>IF(ISCHANGED(Status__c), AND(ISPICKVAL(Status__c, &apos;Cancelled&apos;), ISBLANK( Explanation_of_Cancellation__c)), false)</errorConditionFormula>
        <errorDisplayField>Explanation_of_Cancellation__c</errorDisplayField>
        <errorMessage>Explanation of Cancellation is required to Cancel an Action Item.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TSS_No_access_to_modify_the_action_items</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(ISCHANGED( Service_Category__c ), 
ISCHANGED( Addresses_a_Comment__c ), 
ISCHANGED( Estimated_Completion_Date__c ), 
ISCHANGED( Actual_Completion_Date__c ), 
ISCHANGED( Extent_of_Issue__c ), 
ISCHANGED( Name ), 
ISCHANGED( TSS_Result_Action_Plan__c ), 
ISCHANGED( Other_Service_Category__c ), 
ISCHANGED( Agency_Consulted__c ), 
ISCHANGED( Issue_Description__c ), 
ISCHANGED( Issue_Resolution__c ), AND(ISCHANGED( Status__c ), OR(ISPICKVAL(Status__c, &apos;In Progress&apos;), ISPICKVAL(Status__c, &apos;Completed&apos;)))),
IF(( TSS_Result_Action_Plan__r.Property_Manager__r.Id  != $User.Id), true, false))</errorConditionFormula>
        <errorMessage>You don&apos;t have permissions to modify/change the Action Item details for this Action Plan. Please contact the Property Manager of the Action Plan.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TSS_Not_able_to_create_the_action_items</fullName>
        <active>true</active>
        <errorConditionFormula>IF( ISNEW() ,  IF(TSS_Result_Action_Plan__r.Property_Manager__r.Id !=  $User.Id, true, false), false)</errorConditionFormula>
        <errorMessage>You don&apos;t have permissions to create a new Action Items for this Action Plan. Please contact the Property Manager of the Action Plan.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TSS_Other_Service_Category_is_required</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ISCHANGED(Service_Category__c),AND(ISPICKVAL(Service_Category__c , &apos;Other&apos;), ISBLANK(Other_Service_Category__c )), false)</errorConditionFormula>
        <errorMessage>Other Service Category is required if Service Category is Other.</errorMessage>
    </validationRules>
</CustomObject>
