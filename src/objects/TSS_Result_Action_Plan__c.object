        <description>Field Office is required for GSA owned/lease buildings</description>
        <errorConditionFormula>AND(Non_GSA__c =False ,   ISBLANK( Field_Office__c ))</errorConditionFormula>
        <errorDisplayField>Field_Office__c</errorDisplayField>
        <errorMessage>Field Office is required for every GSA owned or leased building.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TSS_Mark_for_Cancel_requires_expla</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  Mark_for_Cancellation__c , ISBLANK(Explanation_for_Cancellation__c ))</errorConditionFormula>
        <errorDisplayField>Explanation_for_Cancellation__c</errorDisplayField>
        <errorMessage>Explanation for Cancellation is required when Marking an Action Plan for Cancellation.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TSS_Marked_for_cancellation_cant_check</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCHANGED( Mark_for_Cancellation__c ),  PRIORVALUE(Mark_for_Cancellation__c) = true)</errorConditionFormula>
        <errorMessage>Once an Action Plan has been Marked for Cancellation, it can not be undone. Please contact your administrator.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TSS_Occupancy_req_for_GSA_building</fullName>
        <active>true</active>
        <description>Occupancy is required for GSA owned/lease buildings</description>
        <errorConditionFormula>AND(Non_GSA__c =False ,   ISNULL( Occupancy__c ))</errorConditionFormula>
        <errorDisplayField>Occupancy__c</errorDisplayField>
        <errorMessage>Occupancy is required for every GSA owned or leased building.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TSS_Owner_can_change_the_action_plan_sum</fullName>
        <active>true</active>
        <description>validation to ensure that owner should change the action plan summary field</description>
        <errorConditionFormula>AND(ISCHANGED(Action_Plan_Summary__c), IF((OwnerId != $User.Id), true, false))</errorConditionFormula>
        <errorDisplayField>Action_Plan_Summary__c</errorDisplayField>
        <errorMessage>You don&apos;t have permissions to modify/change the Action Plan Summary. Please contact the Property Manager of the Action Plan.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TSS_Property_Manager_required_for_GSA_bu</fullName>
        <active>true</active>
        <description>Property Manager is required for GSA owned/lease buildings</description>
        <errorConditionFormula>AND(Non_GSA__c =False ,   ISBLANK(Property_Manager__c))</errorConditionFormula>
        <errorDisplayField>Property_Manager__c</errorDisplayField>
        <errorMessage>Property Manager is required for every GSA owned or leased building.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TSS_Property_Source_req_for_non_GSA</fullName>
        <active>true</active>
        <description>The Property Source is required for non GSA buildings</description>
        <errorConditionFormula>AND(Non_GSA__c =True ,   ISBLANK( TEXT( FRPP_Source__c  )))</errorConditionFormula>
        <errorDisplayField>FRPP_Source__c</errorDisplayField>
        <errorMessage>The Property Source is required for every non GSA building.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TSS_Region_req_for_GSA_building</fullName>
        <active>true</active>
        <description>Region is required for GSA owned/lease buildings</description>
        <errorConditionFormula>AND(Non_GSA__c =False ,   ISBLANK(   Region__c  ))</errorConditionFormula>
        <errorDisplayField>Region__c</errorDisplayField>
        <errorMessage>Region is required for every GSA owned or leased building.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TSS_Regional_Review_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
AND(NOT(ISBLANK(RegReview_Title__c)), OR(ISBLANK(Associated_Cost__c), ISBLANK(RegReview_Justification__c))),

AND(NOT(ISBLANK(Associated_Cost__c)), OR(ISBLANK(RegReview_Title__c), ISBLANK(RegReview_Justification__c))),

AND(NOT(ISBLANK(RegReview_Justification__c)), OR(ISBLANK(Associated_Cost__c), ISBLANK(RegReview_Title__c))))</errorConditionFormula>
        <errorDisplayField>RegReview_Title__c</errorDisplayField>
        <errorMessage>For Regional consideration, please provide a title, justification, and associated cost.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>TSS_Service_Center_req_for_GSA_building</fullName>
        <active>true</active>
        <description>Service Center is required for GSA owned/lease buildings</description>
        <errorConditionFormula>AND(Non_GSA__c =False ,   ISBLANK(  Service_Center__c  ))</errorConditionFormula>
        <errorDisplayField>Service_Center__c</errorDisplayField>
        <errorMessage>Service Center is required for every GSA owned or leased building.</errorMessage>
    </validationRules>
    <validationRules>
