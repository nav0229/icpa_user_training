        <errorConditionFormula>AND(Non_GSA__c =False ,   ISBLANK(Supervisor__c))</errorConditionFormula>
        <errorDisplayField>Supervisor__c</errorDisplayField>
        <errorMessage>Supervisor is required for every GSA owned or leased building.</errorMessage>
    </validationRules>
