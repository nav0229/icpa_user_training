if(numActItems == 0 &amp;&amp; (txtPlanSummary == &apos;&apos; || txtPlanSummary == undefined)) {
alert(&apos;Please create at least one Action Item and fill in the Action Plan Summary field&apos;);
} 
else if(numActItems == 0) {
alert(&apos;Please create at least one Action Item before Submitting for Approval&apos;);
}
else if (txtPlanSummary == &apos;&apos; || txtPlanSummary == undefined) {
alert(&apos;Action Plan Summary must be filled out before Submitting for Approval&apos;);
}
else if (approvaldate != &apos;&apos;) {
alert(&apos;Action Plan was already approved, no action required&apos;);
}
else {  
alert(&apos;Action Plan meets requirements, please click on the Submit for Approval&apos;);
}</url>
