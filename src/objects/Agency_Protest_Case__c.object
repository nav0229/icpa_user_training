    <validationRules>
        <fullName>APCase_Cannot_Be_Closed</fullName>
        <active>true</active>
        <errorConditionFormula>And( 
Is_Currently_In_Approval_Approcess__c= TRUE,
ISPICKVAL(Case_Closed__c, &apos;Yes&apos;))</errorConditionFormula>
        <errorMessage>The case cannot be closed if the case is still in the approval process flow.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>APClose_Case_Rule</fullName>
        <active>true</active>
        <description>Check if Close Case is marked as &apos;Yes&apos;.</description>
        <errorConditionFormula>ISPICKVAL(Case_Closed__c, &apos;Yes&apos;)
&amp;&amp; NOT (ISCHANGED(Case_Closed__c )) 
&amp;&amp; NOT (ISCHANGED(  AP_Mark_for_Deletion__c ))</errorConditionFormula>
        <errorMessage>Record cannot be updated when Case Closed is marked as &apos;Yes&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Case_Closed_Val_Rule_1</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISCHANGED( Case_Closed__c ),  $Permission.Suspension_Debarment_Approval_User_RE )</errorConditionFormula>
        <errorMessage>You do not have the permissions to close or reopen this case</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MarkforDeletionCheck</fullName>
        <active>true</active>
        <description>Check for Mark for Deletion is checked.</description>
        <errorConditionFormula>AP_Mark_for_Deletion__c = true 
&amp;&amp; NOT ((PRIORVALUE( AP_Mark_for_Deletion__c )) = false)
&amp;&amp; NOT (ISCHANGED(  Case_Closed__c ))</errorConditionFormula>
        <errorMessage>Record cannot be updated when Mark for Deletion is checked.</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>APClose_Case</fullName>
        <availability>online</availability>
        <description>Button to update Close Case and lock record.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Close Case</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/33.0/connection.js&quot;)} 

var AP = new sforce.SObject(&quot;Agency_Protest_Case__c&quot;); 
AP.id = &quot;{!Agency_Protest_Case__c.Id}&quot;; 

AP.Close_Case__c = true; 
result = sforce.connection.update([AP]); 

if ( result[0].getBoolean( &quot;success&quot; ) ) 
{ 
window.location.reload(); 
} 
else 
{ 
alert( result[0].errors.message); 
}</url>
    </webLinks>
