        <type>AutoNumber</type>
    </nameField>
    <pluralLabel>Lease Accruals</pluralLabel>
    <searchLayouts/>
    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>Accroual_Amount_not_Negative_OR_Zero</fullName>
        <active>true</active>
        <errorConditionFormula>Accrual_Amount__c  &lt;= 0</errorConditionFormula>
        <errorMessage>Accrual Amount Cannot be Negative or Zero</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Verify_Amount_is_greater_than_0</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Accrual_Amount__c =0,$User.Alias &lt;&gt;&quot;castiron&quot;)</errorConditionFormula>
        <errorMessage>Value must be greater than 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Verify_Include_Accrual_is_set_on_Digest</fullName>
        <active>true</active>
        <description>Checks the include accrual is set on lease digest.</description>
        <errorConditionFormula>AND(OR(ISPICKVAL( LeaseDigest__r.Include_Accrual__c ,&quot;NO&quot;),ISPICKVAL( LeaseDigest__r.Include_Accrual__c,&quot;&quot;)),$User.Alias &lt;&gt;&quot;castiron&quot;)</errorConditionFormula>
        <errorMessage>Cannot Add Lease Accruals, Include Accruals is not specified on parent Digest</errorMessage>
    </validationRules>
</CustomObject>
