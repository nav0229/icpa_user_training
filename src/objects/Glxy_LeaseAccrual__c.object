    <fields>
        <fullName>Accrual_Amount__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Amount obligated for expected cost.</inlineHelpText>
        <label>Accrual Amount</label>
        <precision>14</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Accrual_PDN_Key__c</fullName>
        <externalId>false</externalId>
        <formula>IF( ISPICKVAL(Accrual_Type__c,&quot;Full&quot;),  PDN__c + TEXT(Accrual_Type__c ),&quot;&quot;)</formula>
        <label>Accrual PDN Key</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Accrual_Type__c</fullName>
        <externalId>false</externalId>
        <inlineHelpText>Identifies accrual cost category (CPI, Real Estate Taxes, BID Taxes, Facilities Rental/Other, Settlement)</inlineHelpText>
        <label>Accrual Type</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Full</fullName>
                    <default>false</default>
                </value>
                <value>
                    <fullName>Partial</fullName>
                    <default>false</default>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>LeaseDigest__c</fullName>
        <externalId>false</externalId>
        <label>Lease Digest</label>
        <referenceTo>Glxy_LeaseDigest__c</referenceTo>
        <relationshipLabel>Lease Accruals</relationshipLabel>
        <relationshipName>Lease_Accruals</relationshipName>
        <relationshipOrder>0</relationshipOrder>
        <reparentableMasterDetail>false</reparentableMasterDetail>
        <trackHistory>false</trackHistory>
