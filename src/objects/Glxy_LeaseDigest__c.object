    <validationRules>
        <fullName>Acceptance_Date_on_Include_Interest</fullName>
        <active>true</active>
        <description>System shall validate that if ‘Include Interest’ has a value of ‘Yes’, a value is returned for the Acceptance Date and the system shall confirm that the Acceptance Date is less than or equal to the Date and Time of the audit information returned by system</description>
        <errorConditionFormula>AND(ISPICKVAL(Include_Interest_Ind__c, &quot;YES&quot;),OR( ISBLANK(AcceptanceDT__c), AcceptanceDT__c&gt;NOW()),$User.Alias &lt;&gt;&quot;castiron&quot;)</errorConditionFormula>
        <errorMessage>The Lease Digest Acceptance Date cannot be NULL or Greater than current Date/Time when Include Interest is set to YES.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Acceptance_Date_on_Include_interest_NO</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISPICKVAL( Include_Interest_Ind__c ,&quot;NO&quot;),NOT(ISBLANK( AcceptanceDT__c )),$User.Alias &lt;&gt;&quot;castiron&quot;)</errorConditionFormula>
        <errorMessage>Acceptance Date must be NULL when Include interest is set to NO.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ISF_Less_than_Agreement_RSF</fullName>
        <active>true</active>
        <errorConditionFormula>AND(OR(IA_RSF__c&lt;0,IA_RSF__c &gt; Agreement_RSF__c),$User.Alias &lt;&gt;&quot;castiron&quot;)</errorConditionFormula>
        <errorMessage>IA RSF Must be greater than 0 and  less than or equal to Lease Agreement RSF</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Included_Interest_NOT_NULL</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(OR(ISPICKVAL( Include_Interest_Ind__c , &quot;YES&quot;),ISPICKVAL( Include_Interest_Ind__c , &quot;NO&quot;)))</errorConditionFormula>
        <errorMessage>Include Interest Indicator cannot be NULL</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NoChange_on_Accrual_on_NO</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCHANGED(Include_Accrual__c) ,ISPICKVAL(PRIORVALUE(Include_Accrual__c), &apos;NO&apos;) )</errorConditionFormula>
        <errorMessage>You cannot change Include Accrual value from &apos;NO&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Verify_if_Urgent_on_Suspend_off</fullName>
        <active>true</active>
        <description>Verifies that if IUgent is set to True, suspend must be false.</description>
        <errorConditionFormula>AND(ISCHANGED(Urgent_Ind__c), Suspend_Ind__c =True, $User.Alias &lt;&gt; &quot;castiron&quot;)</errorConditionFormula>
        <errorMessage>Urgent Indicator cannot be changed if Suspend Flag is True.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Verify_no_existing_Accruals_on_No_value</fullName>
        <active>true</active>
        <description>Check that if Include Accrual is set to no, no accrual child records exist</description>
        <errorConditionFormula>AND( OR(ISPICKVAL(Include_Accrual__c ,&quot;NO&quot;),ISPICKVAL(Include_Accrual__c ,&quot;&quot;)), Count_of_Accruals__c &gt;0,$User.Alias &lt;&gt;&quot;castiron&quot;)</errorConditionFormula>
        <errorMessage>Must remove existing Accrual records before setting this field to No or blank.</errorMessage>
    </validationRules>
