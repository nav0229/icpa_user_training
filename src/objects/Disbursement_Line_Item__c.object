    <validationRules>
        <fullName>Disbursements_Greater_than_Total_Amount</fullName>
        <active>true</active>
        <description>If the Disbursed Amount across all disbursement line items(there will be multiple disbursement line items for a category across multiple disbursements for a Loan) for a given category is &gt; Total Category Amount(on the Loan record) throw an error</description>
        <errorConditionFormula>OR(
 IF(AND(ISPICKVAL(Category__c, &apos;Contents&apos;), (Disbursement__r.Loan__r.Balance_Contents__c + PRIORVALUE(Disbursed_Amount__c )) - Disbursed_Amount__c &lt; 0) , true, false),
 IF(AND(ISPICKVAL(Category__c, &apos;Real Estate (Repair/Replace)&apos;), (Disbursement__r.Loan__r.Balance_Real_Estate_Repair__c + PRIORVALUE(Disbursed_Amount__c )) - Disbursed_Amount__c &lt; 0) , true, false),
 IF(AND(ISPICKVAL(Category__c, &apos;Motor Vehicle&apos;), (Disbursement__r.Loan__r.Balance_Motor_Vehicle__c + PRIORVALUE(Disbursed_Amount__c )) - Disbursed_Amount__c &lt; 0) , true, false),
 IF(AND(ISPICKVAL(Category__c, &apos;Real Estate (Other)&apos;), (Disbursement__r.Loan__r.Balance_Real_Estate_Other__c + PRIORVALUE(Disbursed_Amount__c )) - Disbursed_Amount__c &lt; 0 ) , true, false),
 IF(AND(ISPICKVAL(Category__c, &apos;Repay IHP&apos;), (Disbursement__r.Loan__r.Balance_Repay_IHP__c + PRIORVALUE(Disbursed_Amount__c )) - Disbursed_Amount__c &lt; 0) , true, false),
 IF(AND(ISPICKVAL(Category__c, &apos;Refinancing&apos;), (Disbursement__r.Loan__r.Balance_Refinancing__c + PRIORVALUE(Disbursed_Amount__c )) - Disbursed_Amount__c &lt; 0) , true, false),
 IF(AND(ISPICKVAL(Category__c, &apos;Mitigation&apos;), (Disbursement__r.Loan__r.Balance_Mitigation__c + PRIORVALUE(Disbursed_Amount__c )) - Disbursed_Amount__c &lt; 0) , true, false),
 IF(AND(ISPICKVAL(Category__c, &apos;Contractor\&apos;s Malfeasance&apos;), (Disbursement__r.Loan__r.Balance_Contractor_Malfeasance__c + PRIORVALUE(Disbursed_Amount__c )) - Disbursed_Amount__c &lt; 0) , true, false),
 IF(AND(ISPICKVAL(Category__c, &apos;Interim Financing&apos;), (Disbursement__r.Loan__r.Balance_Interim_Financing__c + PRIORVALUE(Disbursed_Amount__c )) - Disbursed_Amount__c &lt; 0) , true, false),
 IF(AND(ISPICKVAL(Category__c, &apos;Custom - Re&apos;), (Disbursement__r.Loan__r.Balance_Custom_RE__c + PRIORVALUE(Disbursed_Amount__c )) - Disbursed_Amount__c &lt; 0) , true, false),
 IF(AND(ISPICKVAL(Category__c, &apos;Custom - Contents&apos;), (Disbursement__r.Loan__r.Balance_Custom_Contents__c + PRIORVALUE(Disbursed_Amount__c )) - Disbursed_Amount__c &lt; 0) , true, false))</errorConditionFormula>
        <errorDisplayField>Disbursed_Amount__c</errorDisplayField>
        <errorMessage>Total amount disbursed cannot be greater than Maximum eligible amount</errorMessage>
    </validationRules>
</CustomObject>
