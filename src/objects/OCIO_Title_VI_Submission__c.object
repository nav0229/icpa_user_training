    <validationRules>
        <fullName>Delete_LEP_Notif_Other_Info_if_Not_Other</fullName>
        <active>true</active>
        <description>If LEP Notification Method does not include &quot;Other&quot;, then the LEP Notification Method - Other field should be empty.</description>
        <errorConditionFormula>IF(AND(NOT(ISBLANK( OCR_LEP_Notification_Method_Other__c )), NOT(INCLUDES(  OCR_LEP_Notification_Method__c , &apos;Other&apos;))), true, IF(AND((ISBLANK(OCR_LEP_Notification_Method_Other__c)), (INCLUDES( OCR_LEP_Notification_Method__c, &apos;Other&apos;))), true, false))</errorConditionFormula>
        <errorMessage>LEP Notification Method - Other field value is required only if Other is selected in LEP Notification Method field and should be empty otherwise.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Delete_Lang_Other_Info_if_Lang_Not_Other</fullName>
        <active>true</active>
        <description>Language - Other field value is required only if Other is selected in Language Most Encountered field and should be empty otherwise.</description>
        <errorConditionFormula>IF(AND(NOT(ISBLANK(OCR_Lanuage_Other__c)), NOT(INCLUDES(  OCR_Languages_Most_Encountered__c, &apos;Other&apos;))), true, IF(AND((ISBLANK(OCR_Lanuage_Other__c)), (INCLUDES( OCR_Languages_Most_Encountered__c, &apos;Other&apos;))), true, false))</errorConditionFormula>
        <errorMessage>Language - Other field value is required only if Other is selected in Language Most Encountered field and should be empty otherwise.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Delete_Org_Type_Other_Info_if_Not_Other</fullName>
        <active>true</active>
        <description>If Organization Type does not include &quot;Other&quot;, then the Organizational Type - Other field should be empty.</description>
        <errorConditionFormula>IF(AND(NOT(ISBLANK(OCR_Organizational_Type_Other__c)), NOT(INCLUDES( OCR_Organizational_Type__c, &apos;Other&apos;))), true, IF(AND((ISBLANK(OCR_Organizational_Type_Other__c)), (INCLUDES( OCR_Organizational_Type__c, &apos;Other&apos;))), true, false))</errorConditionFormula>
        <errorMessage>Organization Type - Other field value is required only if Other is selected in Organization Type field and should be empty otherwise.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Delete_Trans_Other_Info_if_Not_Other</fullName>
