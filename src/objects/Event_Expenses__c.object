    <validationRules>
        <fullName>Actual_amount_fields_validation</fullName>
        <active>true</active>
        <description>Will not allow the user to enter actual values until after the conference end date.</description>
        <errorConditionFormula>AND(Event_Identifier__r.EA_End_Date__c &gt; TODAY(),NOT(ISBLANK( EA_Actual_Amount__c )))</errorConditionFormula>
        <errorMessage>You cannot enter Actual Amounts before the event end date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Estimated_amount_fields_validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISCHANGED(EA_Estimated_Amount__c),
OR(ISPICKVAL(Event_Identifier__r.EA_Stage__c,&quot;Approved&quot;),
ISPICKVAL(Event_Identifier__r.EA_Stage__c,&quot;Waiting for Actuals&quot;)))</errorConditionFormula>
        <errorMessage>In order to change your estimated amounts yo must clone the conference and re-submit for approval</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_fill_in_reason_for_Other</fullName>
        <active>true</active>
        <description>users must fill in a reason for selecting the &quot;Other&quot; expense category</description>
        <errorConditionFormula>AND(ISPICKVAL( Expense_Category__c , &quot;Other&quot;),  ISBLANK(EA_IF_Other_Explain__c ))</errorConditionFormula>
        <errorMessage>You must provide an explanation of &quot;Other&quot; expense Categories</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_New_Expense_After_Approved</fullName>
        <active>true</active>
        <description>Will not allow a user to enter a new Event Expense once the conference has been approved or canceled</description>
        <errorConditionFormula>AND( 
OR( ISPICKVAL( Event_Identifier__r.EA_Stage__c ,&quot;Approved&quot;), 
ISPICKVAL( Event_Identifier__r.EA_Stage__c ,&quot;Canceled&quot;)), ISNEW())</errorConditionFormula>
        <errorMessage>You cannot add new expenses to an already approved or canceled conference</errorMessage>
    </validationRules>
