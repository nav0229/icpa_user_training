    <validationRules>
        <fullName>Avoid_Duplicate_Standard_PMOAO_Numbers</fullName>
        <active>true</active>
        <description>&quot;77ZA&quot; is reserved PMOAO Number for storage account. Do not let users enter &quot;77ZA&quot; in any other CIS office irrespective of region &amp; MOC.</description>
        <errorConditionFormula>AND( 
 UPPER(PMO_AO_Number__c) = &quot;77ZA&quot;,
 OR(
    AND(
        NOT(Duplicate_PmoAo_Check__c = NULL),
        NOT(Duplicate_PmoAo_Check__c = &quot;77ZA&quot;)),
    NOT(ISPICKVAL(Member_ID__r.Region__c,&quot;&quot;))
   )
)</errorConditionFormula>
        <errorDisplayField>PMO_AO_Number__c</errorDisplayField>
        <errorMessage>PMO/AO Number &quot;77ZA&quot; is preserved for storage account. Please enter any other PMO/AO Number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Fax_Number_must_be_10</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( Fax_Number__c &lt;&gt; &quot;&quot;, 

LEN( 
SUBSTITUTE( 
SUBSTITUTE( 
SUBSTITUTE( 
SUBSTITUTE( 
SUBSTITUTE( 
SUBSTITUTE(  Fax_Number__c , &quot;(&quot;, &quot;&quot;), &quot;)&quot;, &quot;&quot;), &quot;-&quot;,&quot;&quot;),&quot; &quot;,&quot;&quot;),&quot;.&quot;,&quot;&quot;),&quot;/&quot;,&quot;&quot;)) &lt;&gt; 10)</errorConditionFormula>
        <errorMessage>Fax Number must be 10 digits</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Last_Inventory_Date_not_future</fullName>
        <active>true</active>
        <description>Last Inventory Date cannot be future</description>
        <errorConditionFormula>Last_Inventory_Date__c &gt; TODAY()</errorConditionFormula>
        <errorMessage>Last Inventory Date cannot be future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Main_Phone_Number_must_be_10</fullName>
        <active>true</active>
        <errorConditionFormula>AND (Main_Phone_Number__c &lt;&gt; &quot;&quot;, 

LEN( 
SUBSTITUTE( 
SUBSTITUTE( 
SUBSTITUTE( 
SUBSTITUTE( 
SUBSTITUTE( 
SUBSTITUTE( Main_Phone_Number__c , &quot;(&quot;, &quot;&quot;), &quot;)&quot;, &quot;&quot;), &quot;-&quot;,&quot;&quot;),&quot; &quot;,&quot;&quot;),&quot;.&quot;,&quot;&quot;),&quot;/&quot;,&quot;&quot;)) &lt;&gt; 10)</errorConditionFormula>
        <errorMessage>Main Phone Number must be 10 digits</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>PMOAO_Number_should_be_length_of_4</fullName>
        <active>true</active>
        <description>PMOAO Number should be a length of 4 characters</description>
        <errorConditionFormula>AND( LEN(PMO_AO_Number__c ) &lt; 4,  $RecordType.Name = &quot;Senate&quot;)</errorConditionFormula>
        <errorDisplayField>PMO_AO_Number__c</errorDisplayField>
        <errorMessage>PMOAO Number should be a length of 4 characters</errorMessage>
    </validationRules>
