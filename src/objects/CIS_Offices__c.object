DATE(YEAR(Last_Inventory_Date__c)+ 2, MONTH(Last_Inventory_Date__c), DAY(Last_Inventory_Date__c)), 

DATE(YEAR(Last_Inventory_Date__c) + 6, MONTH(Last_Inventory_Date__c), DAY(Last_Inventory_Date__c))

