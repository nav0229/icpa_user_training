    <fields>
        <fullName>CSA_Hours_Remaining__c</fullName>
        <description>Calculates the number of hours remaining for an employee&apos;s CSA for a particular SF 182 Request. The formula is based on a 40-hour average work week.</description>
        <externalId>false</externalId>
        <formula>IF( Total_Hours__c &gt;= 80 || Grand_Total_Training_Costs__c &gt;= 3000, 
  ( IF ( Training_End_Date__c &gt; Today(),
         Total_Hours__c*3, 
         IF ( ABS( Today() - Training_End_Date__c ) &gt; 7, 
              IF ( Total_Hours__c*3 - ABS(((Today() - Training_End_Date__c)/7) * 40) &gt; 0, 
              Total_Hours__c*3 - ABS(((Today() - Training_End_Date__c)/7) * 40),
              0  ), 
              IF ( Total_Hours__c*3 - ABS((Today() - Training_End_Date__c) * 8) &gt; 0, 
              Total_Hours__c*3 - ABS((Today() - Training_End_Date__c) * 8),
              0  ) 
             ) 
       )
   ) , 
-1)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>CSA Hours Remaining</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
