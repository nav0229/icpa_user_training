    <validationRules>
        <fullName>SF182_validation_to_check_the_resubmt</fullName>
        <active>false</active>
        <errorConditionFormula>IF( 
    OR(
        AND( ISPICKVAL( Final_Status__c ,&quot;In Process&quot;), Final_Approver__c = true, Resubmit_Approval_Process__c = true),AND(Final_Approver__c = true, Resubmit_Approval_Process__c = true,ISPICKVAL( Final_Status__c ,&quot;Final Approval&quot;))),false, true)</errorConditionFormula>
        <errorMessage>You cannot resubmit your request at this time.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SF_182_Purchasing_Training_Coordinator</fullName>
        <active>false</active>
        <errorConditionFormula>AND(

($User.Id) = ( SF_182__r.Owner:User.Id ),  (RecordType.DeveloperName = &quot;Purchasing Training Coordinator&quot;)

)</errorConditionFormula>
        <errorMessage>Your Suppose to do modification on this.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SF_182_Training_Completed_Rule</fullName>
        <active>true</active>
        <description>Only allow the Training Completed checkbox to be checked after the Training End date.</description>
        <errorConditionFormula>AND(Training_End_Date__c &gt; TODAY(),  Training_Completed__c = TRUE)</errorConditionFormula>
        <errorDisplayField>Training_Completed__c</errorDisplayField>
        <errorMessage>You may only check the Training Completed field after the Training End Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SF_182_Training_validation_for_pc</fullName>
        <active>true</active>
        <description>Validation for purchasing coordinator when the final approver is true.</description>
        <errorConditionFormula>OR(AND(Final_Approver__c = true, ISBLANK( Purchasing_Training_Coordinator__c ), ISBLANK(Text( Purchasing_Training_Coordinator_Group__c ) ) ) )</errorConditionFormula>
        <errorMessage>Please enter the Purchase Card Holder or Purchase Card Holder Group.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SF_182_Validation_on_Final_Approver</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ($User.Id  =  CreatedById), (Final_Approver__c = True),(   $RecordType.Name !=&quot;SF 182 Training Completed&quot;), NOT( Training_Completed__c))</errorConditionFormula>
        <errorMessage>As the creator of this request you do not have the permissions to edit this field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SF_182_form_check_for_start_and_end_date</fullName>
        <active>true</active>
        <errorConditionFormula>Training_End_Date__c &lt; Training_Start_Date__c</errorConditionFormula>
        <errorDisplayField>Training_End_Date__c</errorDisplayField>
        <errorMessage>Training End Date should be later than Training Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SF_182_validation_for_Location</fullName>
        <active>true</active>
        <errorConditionFormula>OR(

AND (ISPICKVAL( Location__c ,&apos;Off-site&apos;), If_Offsite_list_City__c= &quot;&quot;),

AND (ISPICKVAL( Location__c ,&apos;Off-site&apos;),ISPICKVAL( If_Offsite_list_State__c ,&quot;&quot;))
)</errorConditionFormula>
        <errorMessage>Off-site, list City and Off-site, list State fields are required when the Location is Off Site. Please complete these two fields before saving.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sf_182_Training_is_money_refundable</fullName>
        <active>true</active>
        <description>When ever money Obligated &quot;Yes&quot;. They have to select the Money Refundable</description>
        <errorConditionFormula>IF( 

(
     AND ((ISPICKVAL(Was_Money_Obligated__c, &apos;yes&apos;)),(ISPICKVAL( Is_the_Money_Refundable__c , &quot;&quot;)))

),true,false)</errorConditionFormula>
        <errorDisplayField>Is_the_Money_Refundable__c</errorDisplayField>
        <errorMessage>Please select whether or not the money obligated is refundable.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sf_182_Training_on_Deadline_for_request</fullName>
        <active>true</active>
        <description>When ever &quot; is money refundable&quot; Yes they have to select &quot;Deadline for requesting a refund Date&quot;</description>
        <errorConditionFormula>IF( 

(
   AND ((ISPICKVAL(Was_Money_Obligated__c, &apos;yes&apos;)),(ISPICKVAL( Is_the_Money_Refundable__c , &apos;yes&apos;)),
         ( ISNULL(Deadline_for_requesting_a_refund_Date__c))  || (Deadline_for_requesting_a_refund_Date__c  &lt;= TODAY())

)),true,false)</errorConditionFormula>
        <errorDisplayField>Deadline_for_requesting_a_refund_Date__c</errorDisplayField>
        <errorMessage>If the money is refundable, please enter the deadline date for requesting a refund. The deadline must be later than today&apos;s date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sf_182_Training_validation_for_money_obl</fullName>
        <active>true</active>
        <description>If the Request Status is Cancel . They have to say was the money obligated.</description>
        <errorConditionFormula>IF( 

(
     AND ((ISPICKVAL(Was_Money_Obligated__c, &quot;&quot;)), Cancel_My_Training__c = true)

),true,false)</errorConditionFormula>
        <errorDisplayField>Was_Money_Obligated__c</errorDisplayField>
        <errorMessage>Please select if there was money already obligated for this request.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_Not_to_enter_Approver</fullName>
        <active>false</active>
        <errorConditionFormula>IF( 
  AND (Supervisor__c != &quot;&quot;,Approver_1__c != &quot;&quot; ,Approver_2__c = &quot;&quot; ,Approver_3__c = &quot;&quot;,Approver_4__c =&quot;&quot;, ISPICKVAL(Final_Status__c ,&quot;Final Approval&quot;), Final_Approver__c =True),True,False)</errorConditionFormula>
        <errorMessage>You cannot select another Approver if you are the Final Approver.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_for_Terms_and_Conditions</fullName>
        <active>false</active>
        <errorConditionFormula>IF(
     OR (
AND( ISPICKVAL( Request_Status__c , &quot;Initiating&quot;), Grand_Total_Training_Costs__c  &gt;= 3000, Terms_and_Conditions__c = False),
AND ( ISPICKVAL( Request_Status__c , &quot;Initiating&quot;),Training_Duty_Hours__c   &gt;= 80,Terms_and_Conditions__c = False)),True,False)</errorConditionFormula>
        <errorMessage>Please select the GSA Continuing Service Agreement button and accept the Continued Service Agreement if the cost of your request is 3000.00 or more or your Total Hours are greater 80 or more.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_for_the_Approves</fullName>
        <active>true</active>
        <errorConditionFormula>IF( 
    OR(
	 AND (Supervisor__c != &quot;&quot;,Approver_1__c = &quot;&quot; ,Approver_2__c = &quot;&quot; ,Approver_3__c = &quot;&quot;,Approver_4__c =&quot;&quot;,Final_Approver__c=False),
        AND (Supervisor__c != &quot;&quot; ,Approver_1__c  = &quot;&quot;,Final_Approver__c = True),
       AND (Supervisor__c != &quot;&quot; ,Approver_1__c != &quot;&quot;,Approver_2__c = &quot;&quot; ,Approver_3__c = &quot;&quot;,Approver_4__c =&quot;&quot;,Final_Approver__c =False),
      AND (Supervisor__c != &quot;&quot;,Approver_1__c != &quot;&quot; ,Approver_2__c  = &quot;&quot;,Final_Approver__c = True),
     AND (Supervisor__c != &quot;&quot; ,Approver_1__c != &quot;&quot;,Approver_2__c != &quot;&quot; ,Approver_3__c = &quot;&quot;,Approver_4__c =&quot;&quot;,Final_Approver__c =False),
    AND (Supervisor__c != &quot;&quot;,Approver_1__c != &quot;&quot; ,Approver_2__c != &quot;&quot; ,Approver_3__c  = &quot;&quot;,Final_Approver__c = True),
   AND (Supervisor__c != &quot;&quot; ,Approver_1__c != &quot;&quot;,Approver_2__c != &quot;&quot; ,Approver_3__c != &quot;&quot;,Approver_4__c =&quot;&quot;,Final_Approver__c =False),
  AND (Supervisor__c != &quot;&quot; ,Approver_1__c != &quot;&quot;,Approver_2__c != &quot;&quot; ,Approver_3__c != &quot;&quot;,Approver_4__c =&quot;&quot;,Final_Approver__c =True),
  AND (Supervisor__c != &quot;&quot;,Approver_1__c != &quot;&quot; ,Approver_2__c != &quot;&quot; ,Approver_3__c != &quot;&quot;,Approver_4__c !=&quot;&quot;,Final_Approver__c=False),
 AND (Supervisor__c != &quot;&quot;,Approver_1__c != &quot;&quot; ,Approver_2__c != &quot;&quot; ,Approver_3__c != &quot;&quot;,Approver_4__c !=&quot;&quot;,Final_Approver__c=True)),False,True)</errorConditionFormula>
        <errorMessage>You cannot skip an Approver. Please be sure to follow your organizational structure. For example, a Supervisor should select Approver 1 and Approver 1 should select Approver 2. A Supervisor should not select Approver 2 until Approver 1 has been chosen.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>validation_to_check_the_approver</fullName>
        <active>false</active>
        <errorConditionFormula>IF(
AND(Supervisor__c  &lt;&gt; NUll, Final_Approver__c = True,  ISCHANGED( Approver_1__c )),True, False)</errorConditionFormula>
        <errorMessage>You cannot select the next Approver at this time.</errorMessage>
    </validationRules>
