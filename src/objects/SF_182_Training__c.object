    <fields>
        <fullName>Pending_for_me__c</fullName>
        <description>This field checks if the current user has any training requests pending for approval. The current user can be either Supervisor or Approver 1 - 4 or Purchase Card Holder. This field is being used on the list view filter on the IDP Launch Page link “SF 182 Approver/Supervisor View”</description>
        <externalId>false</externalId>
        <formula>IF ( 

   ( (Supervisor__c  = $User.Id) &amp;&amp; ISPICKVAL(Approval_Status__c, &quot;Pending Supervisor Review&quot;) &amp;&amp;  ISPICKVAL( Request_Status__c , &quot;Approval Process in Progress&quot;) )
|| ( (Approver_1__c = $User.Id) &amp;&amp; ISPICKVAL(Approval_Status__c, &quot;Reviewed by Supervisor&quot;) &amp;&amp;  ISPICKVAL( Request_Status__c , &quot;Approval Process in Progress&quot;))
|| ( (Approver_2__c = $User.Id) &amp;&amp; ISPICKVAL(Approval_Status__c, &quot;Reviewed by Approver 1&quot;) &amp;&amp;  ISPICKVAL( Request_Status__c , &quot;Approval Process in Progress&quot;))
|| ( (Approver_3__c = $User.Id) &amp;&amp; ISPICKVAL(Approval_Status__c, &quot;Reviewed by Approver 2&quot;) &amp;&amp;  ISPICKVAL( Request_Status__c , &quot;Approval Process in Progress&quot;))
|| ( (Approver_4__c = $User.Id) &amp;&amp; ISPICKVAL(Approval_Status__c, &quot;Reviewed by Approver 3&quot;) &amp;&amp;  ISPICKVAL( Request_Status__c , &quot;Approval Process in Progress&quot;)
|| ( (Purchasing_Training_Coordinator__c = $User.Id) &amp;&amp;   ISBLANK(Appropriation_Fund_Direct_Cost__c ) &amp;&amp;  NOT (ISPICKVAL(Request_Status__c, &apos;Initiating&apos;)) )
)

, True, False)</formula>
        <label>Is the request pending on me?</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
