    <validationRules>
        <fullName>Assignee_required_for_2nd_Approval_Blog</fullName>
        <active>true</active>
        <description>Before 2nd Approval is given, &quot;Assignee&quot; should not be entered.  
When 2nd Approval is given, Hidden Field is set to Blog.  When this happens, Assignee is required.</description>
        <errorConditionFormula>OR(AND(Hidden_Field_for_Enabling_Assignee__c = &quot;ALLOW EDIT&quot;,  ISBLANK(Assignee__c), $RecordType.Name = &apos;Blog Request&apos;),

AND(NOT(Hidden_Field_for_Enabling_Assignee__c = &quot;ALLOW EDIT&quot;),  NOT(ISBLANK(Assignee__c)), $RecordType.Name = &apos;Blog Request&apos;))</errorConditionFormula>
        <errorMessage>You must select an Assignee after Final Approval is given.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Close_Date_cannot_be_after_current_date</fullName>
        <active>true</active>
        <description>Eliminates a user from setting a close date later than the current day.</description>
        <errorConditionFormula>Close_Date__c &gt; TODAY()</errorConditionFormula>
        <errorMessage>Your close date cannot be greater than today</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Closed_Date_Must_be_entered_if_closed</fullName>
        <active>true</active>
        <description>Will require a user of the complaint tracker  record type to enter a closed date if the status field is set to one of the closed values.</description>
        <errorConditionFormula>AND(OR( ISPICKVAL( Status__c , &quot;Closed: Resolved&quot;) , ISPICKVAL( Status__c ,&quot;Closed: Unresolved&quot;),  ISPICKVAL(Status__c ,&quot;Closed: Duplicate&quot;), ISPICKVAL( Status__c , &quot;Closed: Litigation&quot;) , ISPICKVAL(Status__c, &quot;Forwarded to OIG for review&quot;)  ), ISBLANK( Close_Date__c ) )</errorConditionFormula>
        <errorMessage>You must select a Close Date if you are selecting one of the closed statuses.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Completed_after_2nd_approval</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(Hidden_Field_for_Enabling_Assignee__c = &quot;ALLOW EDIT&quot;), $RecordType.Name = &apos;Blog Request&apos;, Completed__c = true)</errorConditionFormula>
        <errorMessage>&quot;Completed&quot; can only be checked after 2nd Approval</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Deadline_date</fullName>
        <active>true</active>
        <description>Deadline date cannot be less than current date.</description>
        <errorConditionFormula>NOT( ISPICKVAL(Status__c, &quot;Completed&quot;) ) &amp;&amp; OCIO_CSWR_DeadlineDate__c  &lt;  TODAY()</errorConditionFormula>
        <errorDisplayField>OCIO_CSWR_DeadlineDate__c</errorDisplayField>
        <errorMessage>Deadline date cannot be less than current date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>DueDateCheck</fullName>
        <active>true</active>
        <errorConditionFormula>OCIO_CSWR_DateDueBack__c &lt; OCIO_CSWR_DateSentOut__c</errorConditionFormula>
        <errorDisplayField>OCIO_CSWR_DateDueBack__c</errorDisplayField>
        <errorMessage>Date send out should be less than date due.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_enter_office</fullName>
        <active>true</active>
        <description>Requires the user to enter the office the task was forwarded to if the status of the record is &quot;Tasked to region/offic&quot;</description>
        <errorConditionFormula>AND(
ISPICKVAL( Status__c ,&quot;Tasked To a Region/Office For Review&quot;) ,
 ISBLANK( Office_Forwarded_To__c ))</errorConditionFormula>
        <errorDisplayField>Office_Forwarded_To__c</errorDisplayField>
        <errorMessage>You must Enter the office the complaint has been forwarded to</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Must_input_GSA_office_when_Internal</fullName>
        <active>true</active>
        <description>IF the record type is Complaint Tracker, and the Internal/Extenral picklist value is Internal, then GSA Office must be populated.</description>
        <errorConditionFormula>AND(RecordType.DeveloperName  = &quot;Complaint_Tracker&quot;, ISBLANK( GSA_Office__c) , ISPICKVAL( OCIO_Blog_InternalOrExternal__c , &quot;Internal GSA-Only&quot;) )</errorConditionFormula>
        <errorDisplayField>GSA_Office__c</errorDisplayField>
        <errorMessage>This is an internal complaint, please fill in the complaintants GSA Office</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_Explanation_field_validation</fullName>
        <active>true</active>
        <description>Validate the dependency between Project Type and Other Explanation fields</description>
        <errorConditionFormula>AND 
( 
$RecordType.Name &lt;&gt; &quot;Complaint Tracker&quot;, 
OR(AND(INCLUDES( Project_Type__c,&quot;Other&quot; ), ISBLANK( Other_Explanation__c)) ,
AND(NOT(INCLUDES( Project_Type__c,&quot;Other&quot; )), NOT(ISBLANK( Other_Explanation__c))))
)</errorConditionFormula>
        <errorDisplayField>Other_Explanation__c</errorDisplayField>
        <errorMessage>If you have selected the &quot;Other&quot; as Project Type then you have to input the Other Explanation.

If you have not selected &quot;Other&quot; as project Type then DO NOT input any value in this field.</errorMessage>
    </validationRules>
