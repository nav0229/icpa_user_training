    <validationRules>
        <fullName>Cannot_create_with_out_PATO_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISNEW( ), ISNULL( Package__r.PATO_Approved_Date__c ) )</errorConditionFormula>
        <errorMessage>Continuous Monitoring records cannot be created without a P-ATO Date on the package</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Dates_and_ISSO_Days_complete</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
AND(ISPICKVAL( Status__c , &quot;Completed&quot;), ISBLANK( CSP_Submission_Due_Date__c )),
AND(ISPICKVAL( Status__c , &quot;Completed&quot;), ISBLANK(  CSP_Submitted_Date__c )),
AND(ISPICKVAL( Status__c , &quot;Completed&quot;), ISBLANK(   ISSO_Total_Days__c ))
)</errorConditionFormula>
        <errorMessage>Please make sure CSP Submission Due Date, CSP Submitted Date, and ISSO Total Days are filled in before setting Status to Completed</errorMessage>
    </validationRules>
